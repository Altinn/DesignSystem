{
  "paths" : {
    "source" : {
      "root": "./source/",
      "patterns" : "./source/_patterns/",
      "data" : "./source/_data/",
      "meta": "./source/_meta/",
      "annotations" : "./source/_annotations/",
      "styleguide" : "./styleguidekit-assets-altinn/dist/",
      "patternlabFiles" : "./styleguidekit-mustache-altinn/views/",
      "js" : "./source/js",
      "images" : "./source/images",
      "fonts" : "./source/fonts",
      "css" : "./source/css/"
    },
    "public" : {
      "root" : "./public/",
      "patterns" : "./public/patterns/",
      "data" : "./public/styleguide/data/",
      "annotations" : "./public/annotations/",
      "styleguide" : "./public/styleguide/",
      "js" : "./public/js",
      "images" : "./public/images",
      "fonts" : "./public/fonts",
      "css" : "./public/css"
    }
  },
  "styleGuideExcludes": [
    "maler",
    "sider"
  ],
  "defaultPattern": "all",
  "defaultShowPatternInfo": false,
  "cleanPublic" : true,
  "patternExtension": "mustache",
  "ignored-extensions" : ["scss", "DS_Store", "less"],
  "ignored-directories" : ["scss"],
  "debug": false,
  "ishControlsHide": {
    "s": true,
    "m": true,
    "l": true,
    "full": false,
    "random": true,
    "disco": true,
    "hay": true,
    "mqs": true,
    "find": false,
    "views-all": false,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": false
  },
  "ishMinimum": "320",
  "ishMaximum": "2600",
  "patternStateCascade": ["inprogress", "inreview", "complete", "variations"],
  "patternStates": {
  },
  "patternExportPatternPartials": [],
  "patternExportDirectory": "./pattern_exports/",
  "cacheBust": true,
  "starterkitSubDir": "dist",
  "outputFileSuffixes": {
    "rendered": []
  },
  "plugin-node-custom-output": false,
  "plugin-node-data-inheritance": false,
  "plugin-node-json-tab": false,
  "releaseVersion": "1",
  "pncoConfig": {
    "snippetsDirectory": "./export/snippets/"
  }
}
