      <div class="col-12 col-lg-6">
        {{# tjenesteversjon }} {{> atomer-tekst-input}} {{/ tjenesteversjon }}
      </div>
    </div>
    {{/ tjenestekoder }} {{# fjernkopi }}
    <div class="mb-2">
      {{> atomer-avkrysningsboks }}
    </div>
    {{/ fjernkopi }} {{# premisser }}
    <div class="mb-2">
      {{> atomer-tekstfelt }}
    </div>
    {{/ premisser }} {{# information }}
    <div class="mb-2">
      {{> atomer-tekstfelt }}
    </div>
    {{/ information }} {{# saneringsdato }} {{> molekyler-utsett-tidspunkt }} {{/ saneringsdato }} {{# submit}} {{> atomer-knapp:mb-2 }} {{/ submit}}
