<!-- START: 02-organismer-80-Modal-js-innhold-15-dashboard-sendmelding-grensesnitt -->
<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
        <span class="a-iconText-text-small"></span>
        <span class="a-iconText-text-large">Bestill endring av grensesnitt</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">

    <h2 class="a-h3">Hva ønsker du å bestille?</h2>
    <form data-toggle="validator" id="contactForm">
      <div class="row">
        <div class="col-12 col-sm-8">
          <ul class="nav nav-tabs custom-radio a-radioButtons a-navTabsRadio" role="tablist">
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt1" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Meldingstjeneste over webservice</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt2" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Innsendingstjeneste over webservice</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt3" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Aktivering / deaktivering grensesnitt</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt4" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Innsendingstjeneste til meldingsboks</span>
                </div>
              </a>
            </li>
          </ul>

        </div>
      </div>
    </form>

    <div class="tab-content">
      <div class="tab-pane" id="grensesnitt1" role="tabpanel">
        {{ #tab1 }}
        <h3 class="a-h2">Meldingstjeneste over webservice</h3>
        <form data-toggle="validator" id="contactForm2">
          <div class="row">
            <div class="col-12 col-sm-8">
              <div class="form-group a-form-group mb-2">
                <label class="a-form-label">Hvilket miljø ønsker du grensesnitt til?</label> {{#miljo}} {{> atomer-valgknapp }} {{/miljo}}
              </div>

              {{#navn}} {{> atomer-tekst-input:mb-2 }} {{/navn}} {{#ip}} {{> atomer-tekst-input:mb-2 }} {{/ip}} {{#submit}} {{> atomer-knapp:mb-2 }} {{/submit}}

            </div>
          </div>
        </form>
        {{/tab1 }}
      </div>

      <div class="tab-pane" id="grensesnitt2" role="tabpanel">
        {{ #tab2 }}
        <h3 class="a-h2">SOAP API</h3>
        <form data-toggle="validator" id="contactForm2">
          <div class="row">
            <div class="col-12 col-sm-8">
              <div class="form-group a-form-group mb-2">
                <label class="a-form-label">Hvilket miljø ønsker du grensesnitt til?</label> {{#miljo}} {{> atomer-valgknapp }} {{/miljo}}
              </div>

              {{#navn}} {{> atomer-tekst-input:mb-2 }} {{/navn}} {{#ip}} {{> atomer-tekst-input:mb-2 }} {{/ip}} {{#submit}} {{> atomer-knapp:mb-2 }} {{/submit}}

            </div>
          </div>
        </form>
        {{/tab2 }}
      </div>

      <div class="tab-pane" id="grensesnitt3" role="tabpanel">
        {{ #tab3 }}
        <h3 class="a-h2">TUL og SERES</h3>
        <form data-toggle="validator" id="contactForm2">
          <div class="row">
            <div class="col-12 col-sm-8">
              <div class="form-group a-form-group mb-2">
                <label class="a-form-label">Hvilket verktøy ønsker du grensesnitt til?</label> {{#miljo}} {{> atomer-valgknapp }} {{/miljo}}
              </div>

              {{#visualstudio}} {{> atomer-radioknapp }} {{/visualstudio}}

              <div class="form-group a-form-group mb-2">
                <label class="a-form-label">Er det du som skal ha grensesnitt eller noe andre?</label>
                <form data-toggle="validator" id="contactForm">
                  <div class="row">
                    <div class="col-12 col-sm-8">
                      <ul class="nav nav-tabs custom-radio a-radioButtons a-navTabsRadio" role="tablist">
                        <li class="nav-item">
                          <a class="nav-link" data-toggle="tab" href="#meg" role="tab">
                            <div class="custom-control custom-radio a-custom-radio">
                              <span class="custom-control-label">Det er meg</span>
                            </div>
                          </a>
                        </li>
                        <li class="nav-item">
                          <a class="nav-link" data-toggle="tab" href="#andre" role="tab">
                            <div class="custom-control custom-radio a-custom-radio">
                              <span class="custom-control-label">Det er noen andre</span>
                            </div>
                          </a>
                        </li>
                      </ul>
                    </div>
                  </div>
                </form>

                <div class="tab-content">
                  <div class="tab-pane" id="meg" role="tabpanel">

                  </div>
                  <div class="tab-pane" id="andre" role="tabpanel">

                    {{#navn}} {{> atomer-tekst-input:mb-2 }} {{/navn}} {{#epost}} {{> atomer-tekst-input:mb-2 }} {{/epost}}
                  </div>
                </div>
              </div>
              {{#submit}} {{> atomer-knapp:mb-2 }} {{/submit}}

            </div>
          </div>
        </form>
        {{/tab3 }}
      </div>


    </div>


  </div>
</div>

<!-- END: 02-organismer-80-Modal-js-innhold-15-dashboard-sendmelding-grensesnitt -->