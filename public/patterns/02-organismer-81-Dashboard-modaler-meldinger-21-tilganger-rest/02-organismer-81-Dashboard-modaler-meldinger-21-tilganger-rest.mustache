<!-- START: 02-organismer-81-Dashboard-modaler-meldinger-21-tilganger-rest -->
<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
        <span class="a-iconText-text-small"></span>
        <span class="a-iconText-text-large">{{ modal-title }}</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">

    <form data-toggle="validator" id="contactForm">

      {{# valg }}
        {{> atomer-radioknapp:mb-2 }}
        {{/ valg }}

        {{# applikasjonsnavn }}
          {{!-- Obligatorisk --}}
          {{> atomer-tekst-input:mb-2 }}
          {{/ applikasjonsnavn }}

          {{# orgnummer }}
            {{!-- Obligatorisk, gjerne med regex-validering --}}
            {{> atomer-tekst-input:mb-2 }}
            {{/ orgnummer }}

            {{# environment }}
              {{> molekyler-valgknapper-og-spesifiser }}
              {{/ environment }}

              {{# applikasjonstype }}
                <form id="applikasjonstype">
                  {{!-- Obligatorisk --}}
                  {{> atomer-radioknapp }}
                  <div class="mb-2">
                    <div class="nettleser collapse no-transition">
                      {{> atomer-tekst-input }}
                    </div>
                  </div>
                </form>
                {{/ applikasjonstype }}

                {{# apitype }}
                  <form id="apitype">
                    {{> atomer-radioknapp }}
                  </form>
                  {{/ apitype }}

                  <div class="mt-3 mb-2">
                    {{# submit-deaktivert }}
                      <div class="deaktivert collapse show no-transition">
                        {{> atomer-knapp }}
                      </div>
                      {{/ submit-deaktivert }}
                      {{# submit-tjenesteeier}}
                        <div class="tjenesteeier-neste collapse no-transition">
                          {{> atomer-knapp }}
                        </div>
                        {{/ submit-tjenesteeier}}
                        {{# submit-sluttbruker}}
                          <div class="sluttbruker-neste collapse no-transition">
                            {{> atomer-knapp }}
                          </div>
                          {{/ submit-sluttbruker}}
                  </div>
    </form>
  </div>
</div>

<!-- END: 02-organismer-81-Dashboard-modaler-meldinger-21-tilganger-rest -->