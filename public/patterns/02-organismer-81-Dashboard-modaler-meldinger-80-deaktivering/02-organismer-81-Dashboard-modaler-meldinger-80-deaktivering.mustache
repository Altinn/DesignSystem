<!-- START: 02-organismer-81-Dashboard-modaler-meldinger-80-deaktivering -->
<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
        <span class="a-iconText-text-small"></span>
        <span class="a-iconText-text-large">Bestill sanering / deaktivering av tjeneste</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">

    {{# tjenestenavn }}
      {{!-- Obligatorisk --}}
      <div class="mb-2">
        {{> atomer-tekst-input}}
      </div>
      {{/ tjenestenavn }}

      {{# tjenestetype }}
        {{!-- Obligatorisk --}}
        <div class="mb-2">
          {{> atomer-tekst-input}}
        </div>
        {{/ tjenestetype }}

        {{# options1 }}
          {{!-- Må velge minst én --}}
          <div class="mb-2">
            {{> atomer-avkrysningsboks }}
          </div>
          {{/ options1 }}

          {{# environment }}
            {{!-- Må velge én --}}
            {{> molekyler-valgknapper-og-spesifiser }}
            {{/ environment }}

            {{# tjenestekoder }}
              <div class="row mb-2">
                {{!-- Obligatorisk --}}
                <div class="col-12 col-lg-6">
                  {{# tjenestekode }}
                    {{> atomer-tekst-input}}
                    {{/ tjenestekode }}
                </div>
                <div class="col-12 col-lg-6">
                  {{!-- Obligatorisk --}}
                  {{# utgavekode }}
                    {{> atomer-tekst-input}}
                    {{/ utgavekode }}
                </div>
              </div>
              {{/ tjenestekoder }}

              {{# tjenesteversjon }}
                {{!-- Obligatorisk --}}
                <div class="mb-2">
                  {{> atomer-tekst-input }}
                </div>
                {{/ tjenesteversjon }}

                {{# fjernkopi }}
                  <div class="mb-2">
                    {{> atomer-avkrysningsboks }}
                  </div>
                  {{/ fjernkopi }}

                  {{# premisser }}
                    <div class="mb-2">
                      {{> atomer-tekstfelt }}
                    </div>
                    {{/ premisser }}

                    {{# information }}
                      <div class="mb-2">
                        {{> atomer-tekstfelt }}
                      </div>
                      {{/ information }}

                      {{# saneringsdato }}
                        {{!-- Må velge én --}}
                        {{> molekyler-utsett-tidspunkt }}
                        {{/ saneringsdato }}


                        {{# submit}}
                          {{> atomer-knapp:mb-2 }}
                          {{/ submit}}
  </div>
</div>

<!-- END: 02-organismer-81-Dashboard-modaler-meldinger-80-deaktivering -->