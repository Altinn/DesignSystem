<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
        {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
          <span class="a-iconText-text-large">Bestill endring av grensesnitt</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">

    <h2 class="a-h3">Hva ønsker du å bestille?</h2>
    <form data-toggle="validator" id="contactForm">
      <div class="row">
        <div class="col-12 col-sm-8">
          <ul class="nav nav-tabs custom-radio a-radioButtons a-navTabsRadio" role="tablist">
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt1" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Meldingstjeneste over webservice</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt2" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Innsendingstjeneste over webservice</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt3" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Innsendingstjeneste til meldingsboks</span>
                </div>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" data-toggle="tab" href="#grensesnitt4" role="tab">
                <div class="custom-control custom-radio a-custom-radio">
                  <span class="custom-control-label">Aktivering / deaktivering grensesnitt</span>
                </div>
              </a>
            </li>
          </ul>

        </div>
      </div>
    </form>

    <div class="tab-content">
      <div class="tab-pane" id="grensesnitt1" role="tabpanel">
        {{# tab1 }}
          {{#submit}}
            {{> atomer-knapp:mb-2 }}
          {{/submit}}
          {{/ tab1 }}
      </div>
      <div class="tab-pane" id="grensesnitt2" role="tabpanel">
        {{# tab2 }}
          {{#submit}}
            {{> atomer-knapp:mb-2 }}
          {{/submit}}
          {{/ tab2 }}
      </div>
      <div class="tab-pane" id="grensesnitt3" role="tabpanel">
        {{# tab3 }}
          {{#submit}}
            {{> atomer-knapp:mb-2 }}
          {{/submit}}
          {{/ tab3 }}
      </div>
      <div class="tab-pane" id="grensesnitt4" role="tabpanel">
        {{# tab4 }}
          {{#submit}}
            {{> atomer-knapp:mb-2 }}
          {{/submit}}
          {{/ tab4 }}
      </div>
    </div>
  </div>

  <!-- END: 02-organismer-81-Dashboard-modaler-meldinger-30-grensesnitt -->