<div class="a-page" id="smoothState">

	{{> organismer-header }}

	<div class="container">

		<div class="row">
			<div class="col-sm-12 col-lg-6 col-lg-offset-3 a-paddingTop-x3 a-paddingBottom-x3">
				{{> atomer-sok:input-group-large(search-label-id: "searchLarge-front", search-label: "SÃ¸k etter skjema og annet innhold")}}
			</div>
		</div>

		<section id="hovedinnhold" tabindex="-1">

			<div class="row">
				<div class="col-sm-12 col-lg-10 col-lg-offset-1 a-paddingTop-x3 a-paddingBottom-x3">
					<h1>{{{ header }}}</h1>
					<small>{{{ category }}}</small>
					<br /><br />
					{{> atomer-avsnitt }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-12 col-lg-12 col-lg-offset-0 a-paddingTop-x0 a-paddingBottom-x3">
					{{> organismer-grupperte-kort }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-12 col-lg-10 col-lg-offset-1 a-paddingTop-x0 a-paddingBottom-x3">
					<h2>{{{ header-2 }}}</h2>
					{{> molekyler-trekkspill }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6 col-lg-5 col-lg-offset-1 a-paddingTop-x3 a-paddingBottom-x3">
					<h2>{{{ header-3 }}}</h2>
					{{# help }}
						{{{ help-prefix }}}
						{{> atomer-link }}
					{{/ help }}
				</div>
				<div class="col-sm-6 col-lg-5 col-lg-offset-0 a-paddingTop-x3 a-paddingBottom-x3">
					<h2>{{{ header-4 }}}</h2>
					{{# help-links }}
					  {{> atomer-handlingslink }}
				  {{/ help-links }}
				</div>
			</div>

		</section>

		{{> organismer-footer }}

	</div> <!-- END Container -->

</div>
