<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
        <span class="a-iconText-text-small"></span>
        <span class="a-iconText-text-large">Bestill produksjonssetting av kodeliste</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">
    <form data-toggle="validator" id="contactForm2">
      <div class="row">
        <div class="col-12 col-sm-8">

          {{# kodelistenavn }}
            {{!-- Obligatorisk --}}
            <div class="mb-2">
              {{> atomer-tekst-input }}
            </div>
            {{/ kodelistenavn }}

            {{# lenke }}
              {{!-- Obligatorisk --}}
              <div class="mb-2">
                {{> atomer-tekst-input }}
              </div>
              {{/ lenke }}

              {{# information }}
                {{> atomer-tekstfelt }}
                {{/ information }}

                {{#submit}}
                  {{> atomer-knapp:mb-2 }}
                {{/submit}}

        </div>
      </div>
    </form>
  </div>
</div>
<!-- END: 02-organismer-80-Modal-js-innhold-15-dashboard-sendmelding-prod -->