{
  "paths" : {
    "source" : {
      "root": "./source/",
      "patterns" : "./source/_patterns/",
      "data" : "./source/_data/",
      "meta": "./source/_meta/",
      "annotations" : "./source/_annotations/",
      "styleguide" : "./styleguidekit-assets-altinn/dist/",
      "patternlabFiles" : "./styleguidekit-mustache-altinn/views/",
      "js" : "./source/js/",
      "images" : "./source/images/",
      "fonts" : "./source/fonts/",
      "css" : "./source/css/",
      "epi" : "./source/css/scss/episerver/"
    },
    "public" : {
      "root" : "./public/",
      "patterns" : "./public/patterns/",
      "data" : "./public/styleguide/data/",
      "annotations" : "./public/annotations/",
      "styleguide" : "./public/styleguide/",
      "js" : "./public/js/",
      "images" : "./public/images/",
      "fonts" : "./public/fonts/",
      "css" : "./public/css/"
    }
  },
  "styleGuideExcludes": [
    "maler-infoportal",
    "sider-infoportal",
    "maler-portal",
    "sider-portal"
  ],
  "defaultPattern": "all",
  "defaultShowPatternInfo": false,
  "cleanPublic" : false,
  "cleanOutputHtml": true,
  "exportToGraphViz": false,
  "patternExtension": "mustache",
  "ignored-extensions" : ["scss", "DS_Store", "less"],
  "ignored-directories" : ["scss"],
  "debug": false,
  "ishControlsHide": {
    "s": true,
    "m": true,
    "l": true,
    "full": false,
    "random": true,
    "disco": true,
    "hay": true,
    "mqs": true,
    "find": false,
    "views-all": false,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": false
  },
  "ishMinimum": "320",
  "ishMaximum": "2600",
  "patternStateCascade": [],
  "patternStates": {
  },
  "patternExportPatternPartials": [],
  "patternExportDirectory": "./pattern_exports/",
  "cacheBust": true,
  "starterkitSubDir": "dist",
  "outputFileSuffixes": {
    "rendered": []
  },
  "releaseVersion": "1",
  "pncoConfig": {
    "snippetsDirectory": "./export/snippets/"
  },
  "plugins": {
    "plugin-node-json-tab": {
      "enabled": true,
      "initialized": false
    },
    "plugin-node-data-inheritance": {
      "enabled": true,
      "initialized": false
    },
    "plugin-node-custom-output": {
      "enabled": true,
      "initialized": false
    },
    "plugin-node-uiextension": {
      "enabled": true,
      "initialized": false,
      "options": {
        "stylesheets": false,
        "navLinks": {
          "before": [
          ],
          "after": [
          ]
        },
        "gearLinks": {
          "before": [],
          "beforeSearch": []
        }
      }
    }
  }
}
