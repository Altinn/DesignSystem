<div class="a-page">
	{{> organismer-header }}

	<section id="content" tabindex="-1">

		<h1 class="sr-only">Altinn - enklere dialog med det offentlige</h1>

	  {{> felles-komponenter-jumbotron }}

	  <div class="container">
	    <div class="row mb-3">
	      <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
	        <h2 class="a-h3">
	          {{{ list-header }}}
	        </h2>

	        {{# list-last-used-forms }}
	          {{> felles-komponenter-liste-unummerert:mb-2 }}
	        {{/ list-last-used-forms }}

					{{#showAllForms}}
            {{> felles-komponenter-link}}
          {{/showAllForms}}
	      </div>
	    </div>

	    <div class="row py-4">

	      <div class="col-10 offset-1 col-sm-6 offset-sm-0 col-lg-4 offset-lg-2">
	        {{# mediaBlock1 }}
	          {{> molekyler-bildeblokk-innhold-under }}
	        {{/ mediaBlock1 }}
	      </div>

	      <div class="col-10 offset-1 col-sm-6 offset-sm-0 col-lg-4">
	        {{# mediaBlock2 }}
	          {{> molekyler-bildeblokk-innhold-under }}
	        {{/ mediaBlock2 }}
	      </div>
	    </div>

			<div class="row py-4">
	      <div class="col-lg-8 offset-lg-2">
					{{# newslist }}
						{{> felles-komponenter-artikkelliste }}
					{{/ newslist }}
	      </div>
	    </div>
		</div>

	</section>

	{{> organismer-footer }}

</div>

{{> molekyler-hjelp-container }}
