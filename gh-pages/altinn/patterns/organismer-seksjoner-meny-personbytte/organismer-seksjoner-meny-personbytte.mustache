<!-- Begin #a-dropdown-personswitchList - -->
<div class="dropdown-menu a-dropdown-personswitchList a-dropdownTriangle">

  <div class="pt-5 pl-3 pr-3">

    {{> felles-komponenter-link(linkClass: "a-link-logout", link-text: "Logg ut", link-url: "/patterns/04-sider-infoportal-01-forside-00-forside/04-sider-infoportal-01-forside-00-forside.html") }}

    {{# personswitch-current }}
      <div class="row">
        <div class="col-sm-12 px-0 mb-2">
          {{> molekyler-person-privat }}
        </div>
      </div>
    {{/ personswitch-current }}

    {{# profile-text }}
      <p class="{{ profile-text-class }}">
        {{ profile-text }}
      </p>
    {{/ profile-text }}

    {{# personswitch-search }}
      {{> felles-komponenter-tekst-input }}
      {{# no-results-text }}
        <p class="a-fontBold a-fontSizeXS">
          {{ no-results-text }}
        <p>
      {{/ no-results-text }}
    {{/ personswitch-search }}


    {{# show-favourites }}
      <div class="row mt-1 px-md-2 px-sm-1">
        <div class="col-md col-12 mb-md-4 mb-1">
            {{> organismer-favoritter}}
        </div>
      </div>
    {{/ show-favourites }}

  {{# personswitch-button }}
    {{> felles-komponenter-knapp }}
  {{/ personswitch-button }}

  </div>

  {{# personswitch-overview }}
    {{> organismer-personbytte:px-1}}
  {{/ personswitch-overview }}


</div>
