							{{> felles-komponenter-sidetittel:a-h2|a-fontReg }}
						{{/ third-page-title }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 mb-3 ">

					{{# messages2 }}
