<div class="a-page">
	{{> organismer-header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> felles-komponenter-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<section id="content" tabindex="-1">

		<div class="container a-searchResults a-offset-alignContent">
				<div class="row">
					<div class="col-12 col-lg-10 offset-lg-1">
						{{# searchWhere}}
							{{> molekyler-faner-og-knapp }}
						{{/ searchWhere}}
						<h1 class="a-h3 a-fontReg mt-2"><b>149 treff</b> p√• <b>"MVA"</b></h1>
					</div>
				</div>

				<div class="row">
					<div class="col-12 px-0 px-sm-1">
						<form class="a-searchResults-filter">
							<fieldset>
								<legend class="a-legend">Begrens treffet:
								</legend>
								<div class="a-searchResults-filter-options">
									{{# filter-search }}
										{{> felles-komponenter-valgknapp-ett-alternativ:a-switch--small }}
									{{/ filter-search }}
								</div>
							</fieldset>
						</form>
					</div>
				</div>

				{{# hit-form-1 }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-3 {{a-searchResults-lastOfType}}">
							{{> felles-komponenter-ikon-link }}
					</div>
				</div>
				{{/ hit-form-1 }}

				{{# hit-article-1 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> felles-komponenter-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-1 }}

				{{# hit-form-15 }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-3 a-borderBottomDark pt-2 {{a-searchResults-lastOfType}}">
							{{> felles-komponenter-ikon-link }}
					</div>
				</div>
				{{/ hit-form-15 }}

				{{# hit-article-2 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark {{a-searchResults-lastOfType}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> felles-komponenter-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-2 }}

				{{# infoAboutMail }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-0">
							{{> felles-komponenter-bildeblokk:a-cardImage-border|a-cardImage-largePadding }}
					</div>
				</div>
				{{/ infoAboutMail }}

				{{# hit-form-2 }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-3 pt-2 {{a-searchResults-lastOfType}}">
							{{> felles-komponenter-ikon-link }}
					</div>
				</div>
				{{/ hit-form-2 }}

				{{# hit-article-3 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark pt-2 {{a-searchResults-lastOfType}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> felles-komponenter-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-3 }}

				{{# loadmoreSearch }}
				<div class="row">
					<div class="col-sm-12">
							{{> felles-komponenter-handlingsknapp }}
					</div>
				</div>
				{{/ loadmoreSearch }}


			</div> <!-- END Container -->

	</section>

	{{> organismer-footer }}
</div>
