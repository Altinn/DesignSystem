<div class="a-page">
  {{# header }}
    {{> organismer-header }}
  {{/ header }}

  <section id="content" tabindex="-1">

    {{> felles-komponenter-jumbotron }}

    <div class="container">

      {{> molekyler-illustrasjonslenke:mt-4|a-dontPrint }}

      {{# help-questions }}
        <div class="row mt-6">
          <div class="col-sm-12 col-lg-10 offset-lg-1">
            {{> felles-komponenter-seksjonstittel }}
          </div>
  			</div>
        <div class="row">
            <div class="col-sm-12">
              {{> felles-komponenter-trekkspill-overskrifter:a-accordion-large--offset }}
  			   </div>
        </div>
      {{/ help-questions }}

      {{# kontakt }}
      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1">
          {{> felles-komponenter-bildeblokk:mt-5 }}
        </div>
      </div>
      {{/ kontakt }}

      {{# kontaktskjema }}
        {{> felles-komponenter-modal }}
      {{/ kontaktskjema }}

  	</div><!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>


{{> molekyler-hjelp-container }}
