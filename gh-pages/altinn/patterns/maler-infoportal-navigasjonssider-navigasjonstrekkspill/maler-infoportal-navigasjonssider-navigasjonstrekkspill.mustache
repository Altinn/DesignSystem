<div class="a-page">
	{{> organismer-header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> felles-komponenter-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<section id="content" tabindex="-1">

		<div class="container">
				<div class="row">
					<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">

							{{# article-breadcrumb }}
								{{> felles-komponenter-smulesti }}
							{{/ article-breadcrumb }}

							{{> felles-komponenter-sidetittel }}

							{{# hasIntro}}
								{{> felles-komponenter-intro-tekst }}
							{{/ hasIntro}}

					</div>
				</div>

				{{# help-questions }}
				<div class="row">
					<div class="col-sm-12 col-lg-10 offset-lg-1">
						{{> felles-komponenter-seksjonstittel }}
					</div>
				</div>
				<div class="row">
						<div class="col-sm-12">
							{{> felles-komponenter-trekkspill-overskrifter:a-accordion-large--offset }}
					 </div>
				</div>
				{{/ help-questions }}

				<div class="row">
					<div class="col-sm-12 col-lg-10 offset-lg-1">
						{{> felles-komponenter-bildeblokk:mt-5 }}
					</div>
				</div>

			</div> <!-- END Container -->

	</section>

	{{> organismer-footer }}
</div>
