<div id="profile-selection">
  <div class="row mb-1">
    <div class="col-lg-4">
      {{# profile-sok }}
        {{> felles-komponenter-tekst-input }}
      {{/ profile-sok }}
    </div>
  </div>

  <div class="row">
    <div class="col-12 col-lg-4">
      {{# select-all }}
        {{> felles-komponenter-avkrysningsboks:d-inline }}
      {{/ select-all }}
      {{# selected-items }}
        {{> felles-komponenter-merkelapp:d-inline }}
      {{/ selected-items}}
    </div>

    <div class="col-12 col-lg-2">
      {{# toggle-selected-items }}
        {{> felles-komponenter-avkrysningsboks }}
      {{/ toggle-selected-items }}
    </div>

    <div class="col-12 col-lg-3">
      {{# select-subunits }}
        {{> felles-komponenter-avkrysningsboks }}
      {{/ select-subunits }}
    </div>
    <div class="col-12 col-lg-3">
      {{# select-inactive }}
        {{> felles-komponenter-avkrysningsboks }}
      {{/ select-inactive }}
    </div>
  </div>

  {{# checkbox-list }}
    {{> felles-komponenter-nestede-avkrysningsbokser }}
  {{/ checkbox-list }}
</div>