<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                 {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
                <span class="a-iconText-text-large">{{ modal-title }}</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">
      
      <form data-toggle="validator" id="contactForm">
        {{# bruker }}
          {{> felles-komponenter-avkrysningsboks }}
          <div id="bruker3" class="collapse">
            {{# kontakt1 }}
              {{> organismer-dashboard-kontaktinfo }}
            {{/ kontakt1 }}
          </div>
        {{/ bruker }}

        <div class="form-group a-form-group my-2">
          <label class="a-form-label">Hvilke verktøy ønskes det tilgang til?</label>
            {{# miljo}}
            {{> felles-komponenter-valgknapp }}
            {{/ miljo}}
        </div>

        {{# visualstudio}}
          {{> felles-komponenter-avkrysningsboks }}
        {{/ visualstudio}}

        {{# submit}}
          {{> felles-komponenter-knapp:mb-2 }}
        {{/ submit}}
      </form>
  </div>
</div>
