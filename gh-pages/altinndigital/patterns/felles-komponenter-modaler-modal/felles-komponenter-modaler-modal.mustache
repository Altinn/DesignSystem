<div class="a-modal fade modal {{# modal-isSingel }} a-fade--appear {{/ modal-isSingel }}" id="{{modal-id}}" role="dialog" tabindex="-1" {{# modal-label }} aria-labelledby="{{ modal-label }}" {{/ modal-label }}>
  <div class="modal-dialog modal-lg" role="document">
    <div class="container a-p-static">
      <div class="row">
        <div class="col-xl-12">
          {{> felles-komponenter-modal-header }}
        </div>
      </div>
      <div class="row">
        <div class="col-xl-10 offset-xl-1 a-p-static">
          {{# modal-navbar-override }}
            {{> felles-komponenter-modal-navbar }}
          {{/ modal-navbar-override }}
          <div class="a-modal-content-target">
            <!-- The content will be injected here -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
