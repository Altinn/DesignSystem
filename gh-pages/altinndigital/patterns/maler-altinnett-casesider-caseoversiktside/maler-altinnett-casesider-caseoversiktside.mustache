<div class="a-page">
    {{# header }}
        {{> organismer-header-altinnett }}
    {{/ header }}

    <div class="container">

        <div class="row mb-6">
            <div class="col-10">
                <h1 class="a-fontBold a-blueDarkerText a-fontSizeXL">{{ pageTitle }}</h1>
                <p class="a-blueDarkerText a-fontSizeXL">{{ pageIngress }}</p>
            </div>
        </div>

        {{# cases }}
            {{#casePreview}}
                {{> molekyler-altinnett-case-preview }}
            {{/casePreview}}
            {{#caseCard}}
                {{> molekyler-altinnett-case-card }}
            {{/caseCard}}
        {{/ cases }}

    </div>


    {{# footer }}
        {{> organismer-footer-altinnett }}
    {{/ footer }}
</div>