<div class="modal-content">
  <div class="modal-header a-modal-header">
      <div class="a-iconText a-iconText-background a-iconText-large">
          <div class="a-iconText-icon">
              <i class="ai ai-send
                a-icon" aria-hidden="true"></i>
          </div>
          <h1 class="a-iconText-text mb-0">
               {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
              <span class="a-iconText-text-large">{{ modal-title }}</span>
          </h1>
      </div>
  </div>
  <div class="modal-body a-modal-body">
      
    <form data-toggle="validator" id="contactForm">

      {{# valg }}
        {{> felles-komponenter-radioknapp:mb-2 }}
      {{/ valg }}

      {{# applikasjonsnavn }}
      {{!-- Obligatorisk --}}
        {{> felles-komponenter-tekst-input:mb-2 }}
      {{/ applikasjonsnavn }}

      {{# orgnummer }}
      {{!-- Obligatorisk, gjerne med regex-validering --}}
        {{> felles-komponenter-tekst-input:mb-2 }}
      {{/ orgnummer }}

      {{# environment }}
        {{> felles-komponenter-valgknapper-og-spesifiser }}
      {{/ environment }}

      {{# applikasjonstype }}
        <form id="applikasjonstype">
          {{!-- Obligatorisk --}}
          {{> felles-komponenter-radioknapp }}
          <div class="mb-2">
            <div class="nettleser collapse no-transition">
              {{> felles-komponenter-tekst-input }}
            </div>
          </div>
        </form>
      {{/ applikasjonstype }}

      {{# apitype }}
        <form id="apitype">
          {{> felles-komponenter-radioknapp }}
        </form>
      {{/ apitype }}

      <div class="mt-3 mb-2">
        {{# submit-deaktivert }}
          <div class="deaktivert collapse show no-transition">
            {{> felles-komponenter-knapp }}
          </div>
        {{/ submit-deaktivert }}
        {{# submit-tjenesteeier}}
        <div class="tjenesteeier-neste collapse no-transition">
          {{> felles-komponenter-knapp }}
        </div>
        {{/ submit-tjenesteeier}}
        {{# submit-sluttbruker}}
          <div class="sluttbruker-neste collapse no-transition">
            {{> felles-komponenter-knapp }}
          </div>
        {{/ submit-sluttbruker}}
      </div>
    </form>
  </div>
</div>
