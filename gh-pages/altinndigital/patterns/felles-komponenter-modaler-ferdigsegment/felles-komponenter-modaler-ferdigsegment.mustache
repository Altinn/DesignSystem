<div class="{{ styleModifier }}">
  <div class="modal-table-segment-box"></div>
  <div class="modal-table-segment {{ segment-fixed }}">
    {{# include-container }}<div class="container">{{/ include-container }}
      <div class="float-right mt-2">
        {{# ferdigsegment-done }}
          <span class="mr-3">
            {{> felles-komponenter-knapp:a-js-enableWhenValid }}
          </span>
        {{/ ferdigsegment-done}}
        {{# ferdigsegment-cancel }}
          <span class="mr-{{ margin-right-multiplier }}">
            {{> felles-komponenter-knapp-link }}
          </span>
        {{/ ferdigsegment-cancel }}
      </div>
    {{# include-container }}</div>{{/ include-container }}
  </div>
</div>
