<section class="an-dashboard-alerts mb-4">
  <div class="d-flex">
    <div class="flex-grow-1">
      {{# dashboard-alertTitle }}
      {{> felles-komponenter-seksjonstittel:a-fontMedium }}
    {{/ dashboard-alertTitle }}
    </div>
    <div class="align-self-center pb-1 text-right">
      {{# dashboard-alertSubscribe }}
        {{> felles-komponenter-link }}
      {{/ dashboard-alertSubscribe }}
    </div>
  </div>

  {{# dashboard-addAlert }}
    {{> felles-komponenter-handlingsknapp }}
  {{/ dashboard-addAlert }}

  {{# dashboard-addAlert-modal }}
    {{> felles-komponenter-modal }}
  {{/ dashboard-addAlert-modal }}

  {{# dashboard-subscribe-modal }}
    {{> felles-komponenter-modal }}
  {{/ dashboard-subscribe-modal }}

  {{# dashboard-alerts }}
    {{> felles-komponenter-varsel }}
  {{/ dashboard-alerts }}

  <div class="mt-1">
    {{# dashboard-viewMoreAlerts }}
    {{> felles-komponenter-link }}
    {{/ dashboard-viewMoreAlerts }}
  </div>

</section>