<ul class="a-item-list-icon {{ styleModifier }}">
  {{# item-list }}
    <li class="{{ item-list-class }}" tabindex="0"
      {{# icon-onclick }} onClick="{{{ icon-onclick }}}" {{/ icon-onclick }}
      aria-label="{{{ list-item-aria-label }}}"
    >
      <div class="row">
        <div class="col-10">
          <span class="a-js-listItemText">
            {{{ list-item-text }}}
          </span>
        </div>
        <div class="col text-right">
          {{> atomer-ikon }}
        </div>
      </div>
    </li>
  {{/ item-list }}
</ul>