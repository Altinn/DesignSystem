<a data-toggle="collapse" 
    data-parent="#filterSkjema" 
    data-target="#{{{ collapse-filter-id }}}" 
    href="#" 
    aria-expanded="false" 
    class="a-collapse-title {{ styleModifier }} {{# toggle-texts }} toggle-collapse-text {{/ toggle-texts }} {{# no-decoration }} no-decoration {{/ no-decoration }} {{# blue-text-right }} button-blue-no-underline mr-5 float-right {{/ blue-text-right }} collapsed">
    {{# show-arrow }} <span class="a-dropdownArrow"></span> {{/ show-arrow }}

    {{# collapse-primary-text }}
        <span class="primary-collapse-text">
            {{{ collapse-primary-text }}}
        </span>
    {{/ collapse-primary-text }}
    {{# collapse-secondary-text }}
        <span class="secondary-collapse-text">
            {{{ collapse-secondary-text }}}
        </span>
    {{/ collapse-secondary-text }}
</a>
