<a href="javascript:void(0)"
  tabindex="0"
  class="a-linkArea {{ open-popover-class }}"
  role="button"
  data-toggle="popover"
  data-animation="false"
  data-target="#{{ popover-data-target }}"
  data-placement="bottom"
  data-trigger="{{ popover-trigger }}{{^ popover-trigger }}click{{/ popover-trigger }}"
  {{# popover-class }}data-popover-class="{{ popover-class }}"{{/ popover-class }}
  {{# popover-title }} title="{{ popover-title }}" {{/ popover-title }}
  {{# popover-content }} data-content="{{ popover-content }}" {{/ popover-content }}
  data-popover-content="{{ popover-content-id }}">
  {{# popover-link-text-class }}<span class="{{ popover-link-text-class }}">{{/ popover-link-text-class }}
    {{{ popover-link-text }}}
  {{# popover-link-text-class }}</span>{{/ popover-link-text-class }}
  {{# popover-link-icon }}
    {{> atomer-ikon }}
  {{/ popover-link-icon }}
  {{# altText }}
    <span class="sr-only">{{ altText }}</span>
  {{/ altText }}
</a>

{{# popover-list-content }}
  <div id="{{ popover-content-id }}" style="display: none">
    <div class="container">
      <div class="row">
        <div class="col-xl-10 offset-xl-1">
          <div class="a-modal-body">
            {{> molekyler-liste-med-beskrivelse }}
          </div>
        </div>
      </div>
    </div>
  </div>
{{/ popover-list-content}}

{{# popover-list-with-icon-content }}
  <div id="{{ popover-content-id }}" style="display: none">
    <span>{{ popover-title }}</span>
    <div class="{{ popover-list-class }}">
      {{> atomer-liste-unummerert-ikoner }}
    </div>
  </div>
{{/ popover-list-with-icon-content}}

{{# popover-description-content}}
  <div id="{{ popover-content-id }}" style="display: none">
    <div class="container">
      <div class="row">
        <div class="col-xl-10 offset-xl-1">
          <div class="a-modal-body">
            {{> molekyler-tekstinnhold-beskrivelse }}
          </div>
        </div>
      </div>
    </div>
  </div>
{{/ popover-description-content}}

{{# popover-various-content }}
  <div id="{{ popover-content-id }}" style="display: none">
      {{> atomer-varsel-innhold }}
  </div>
{{/ popover-various-content}}
