<a
  {{# link-url }} href="{{{ link-url }}}" {{/ link-url }}
  class="{{ link-class }} {{ styleModifier }}"
  {{# data-toggle }} data-toggle="{{data-toggle}}" {{/ data-toggle }}
  {{# data-action }} data-action="{{data-action}}" {{/ data-action }}
  {{# data-url }} data-url="{{{data-url}}}" {{/ data-url }}
  {{# data-target }} data-toggle="collapse" data-target="#{{ data-target }}" aria-controls="{{ data-target }}" {{/ data-target }}
  {{# data-set-selected-target }} data-set-selected-target="{{ data-set-selected-target }}"{{/ data-set-selected-target }}
  {{# data-parent }} data-parent="#{{ data-parent}}" {{/ data-parent }}
  {{# onclick }} onclick="{{{ onclick }}}" {{/ onclick }}{{# aria-expanded }} aria-expanded="{{ aria-expanded }}"{{/ aria-expanded }}>
  {{# link-icon-left }}
   {{> atomer-ikon }}
  {{/ link-icon-left }}
  {{{ link-text }}}
  {{# sr-text }}<span class="sr-only">{{ sr-text }}</span>{{/ sr-text }}
  {{# link-icon }}
    {{> atomer-ikon }}
  {{/ link-icon }}
</a>