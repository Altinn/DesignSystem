		"section-class": "a-js-genericSearch",
		"section-source": "../../data/skjenkebevilling.json",
		"section-title": "Hvilken kommune skal du søke i?",
		"modal-input-keyword": {
			"input-label-class": "sr-only",
			"input-type": "search",
			"text-input-id": "kommune",
			"isLarge": true,
			"isHidden": true,
			"top-label": "Kommune:",
			"text-input-placeholder": "Søk etter kommune..."
		},
		"modal-loader": true,
		"modal-empty": {
			"paragraph-class": "a-js-noResults",
			"paragraph-content": "Ingen treff. Prøv å søke med andre stikkord."
		},

		"resultList": {
			"rows": [
				{
					"additional-classes": "a-dotted a-clickable a-list-hasRowLink",
					"isRowLink": true,
          "isRowLink": {
						"rowLink": "%URL%"
					},
					"columns": [
						{
							"additional-classes": "col-12",
							"column-items": [
								{
									"column-text": {
										"text": "%NAME%",
										"text-extra": {
											"text-extra": "(%PARENT%)",
											"classes": "a-dimText7"
										}
									}
								}
							]
						}
					]
				}
			]
		}
	}

