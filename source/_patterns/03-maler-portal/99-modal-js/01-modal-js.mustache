<button class="a-btn" data-toggle="altinn-modal" data-action="load" data-url="{{{ page-url }}}" data-target="#modal">Ã…pne modal</button>

<div class="a-modal fade modal" id="modal" role="dialog" tabindex="-1" {{# modal-label }} aria-labelledby="{{ modal-label }}" {{/ modal-label }}>
  <div class="modal-dialog modal-lg" role="document">
    <div class="container a-p-static">
      <div class="row">
        <div class="col-xl-12">
          {{> molekyler-modal-header }}
        </div>
      </div>
      <div class="row">
        <div class="col-xl-10 offset-xl-1 a-p-static">
          {{# modal-navbar-override }}
            {{> molekyler-modal-navbar }}
          {{/ modal-navbar-override }}
          <div class="a-modal-content-target">
            <!-- The content will be injected here -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
