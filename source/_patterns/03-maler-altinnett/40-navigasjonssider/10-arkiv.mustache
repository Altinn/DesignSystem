<div class="a-page">
	{{# header }}
		{{> organismer-header-altinnett }}
	{{/ header }}

	<main id="content" tabindex="-1">

		<div class="container">
			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">

					{{> atomer-sidetittel }}
          {{> atomer-intro-tekst }}

				</div>
			</div>

			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3 {{articlesContainerClass}}">

					{{# list-article }}
							{{> molekyler-lenketittel-med-forklaring }}
					{{/ list-article }}

					{{# list-article-loadmore }}
				    {{> atomer-handlingsknapp }}
				  {{/ list-article-loadmore }}

				</div>
			</div>

			<div class="row mt-4">
				<div class="col-sm-12 col-lg-10 offset-lg-1">
					{{> organismer-abonnere-driftsmeldinger }}
				</div>
			</div>

		</div> <!-- END Container -->

	</main>

	{{# footer }}
		{{> organismer-footer-altinnett }}
	{{/ footer }}
</div>
