{
  "title": "Altinnett",
  "themeCss": "../../css/style.dist.altinnett.css",
  "themeJs": "../../js/altinnettPrototyping.js",
  "themeJsVendor": "../../js/altinnettPrototyping.vendor.js",
  "bodyClass": "project-altinnett a-bgGrey",
  "header": {
    "header-color": "a-darkBackground an-bgBlack",
    "headerlogo": "../../images/altinnett-logo-white.svg",
    "ekstra-menypunkt-1": "Seksjonsforside",
    "url-ekstra-menypunkt-1": "link.sider-altinnett-seksjonsforside",
    "login-icon": {
      "icon-class": "ai-keyhole-circle a-personSwitcher-icon"
    },
    "login-name": "Logg inn"
  },
  "profile-login-prefix": "Profil for",
  "profile-login-name": "Navn Navnesen",
  "profile-login-organization": "Leverandør - [Organisasjonsnavn]",
  "accordion-large-id": "accordion",
  "subscriber": {
    "yourProfilePanel": {
      "collapseID": "myContactInfo",
      "dataParent": "#accordion",
      "collapse-panel-header-id": "myContactInfoHeader",
      "search-rights": false,
      "isModal": false,
      "link-done": false,
      "collapseHeader": {
        "variation-class": "a-iconText-shadow a-iconText-large a-collapseBtn collapsed",
        "hasIcon": {
          "icon-prefix": "reg",
          "icon-class": "reg-private",
          "icon-extra-class": "a-icon"
        },
        "hasTextSmall": false,
        "isHeader": {
          "isH2": true
        },
        "icontext": "Din varslingsprofil"
      }
    }
  },
  "organizationProfilePanel": {
    "collapse-panel-header-id": "EmployeeHeader",
    "collapseID": "accordionItemEmployees",
    "dataParent": "#accordion",
    "collapseHeader": {
      "variation-class": "a-iconText-shadow a-iconText-large a-collapseBtn collapsed",
      "hasIcon": {
        "icon-prefix": "reg",
        "icon-class": "reg-corp",
        "icon-extra-class": "a-icon"
      },
      "hasTextSmall": false,
      "isHeader": {
        "isH2": true
      },
      "icontext": "Varslingsprofil for Leverandør - [Organisasjonsnavn]"
    }
  },
  "alertListPanel": {
    "collapseID": "varslinger",
    "isLarge": "a-collapsePanel--large",
    "collapse-panel-header-id": "alertListHeader",
    "dataParent": "#accordion",
    "collapseHeader": {
      "variation-class": "a-iconText-shadow a-iconText-large a-collapseBtn collapsed",
      "hasIcon": {
        "icon-prefix": "reg",
        "icon-class": "reg-server",
        "icon-extra-class": "a-icon"
      },
      "hasTextSmall": false,
      "icontext": "Varslinger",
      "scheduledAlerts": true,
      "previousAlerts": true
    }
  },
  "unsubscriber": {
    "subscribeLink": {
      "link-text": "Abonner på varslinger",
      "link-class": "a-btn",
      "link-url": "javascript:void(0)",
      "sr-text": false,
      "data-toggle": "altinn-modal",
      "data-action": "load",
      "data-url": {
        "data-url": "../../patterns/02-organismer-80-Modal-js-innhold-80-altinnett-varsling-pamelding/02-organismer-80-Modal-js-innhold-80-altinnett-varsling-pamelding.markup-only.html"
      },
      "data-target": "modal"
    }
  },
  "firstLink": {
    "link-text": "Gå til tjenestekalender",
    "link-url": "javascript:void(0)",
    "sr-text": false
  },
  "secondLink": {
    "link-text": "Self-service portal",
    "link-url": "javascript:void(0)",
    "sr-text": false
  },
  "thirdLink": {
    "button-text": "Logg ut",
    "button-class": "a-btn-link a-linkDanger",
    "display-as-link": true,
    "onclick": {
      "onclick": "location.href='link.sider-altinnett-forside'"
    }
  },
  "subscribeModal": {
    "modal-isSingel": true,
    "show-login": false,
    "display-as-dropdown": false,
    "logo": {
      "logo": "../../images/altinnett-logo-black.svg"
    },
    "logo-class": "a-modal-top-logo",
    "login-show": false,
    "icon": "group_add",
    "hasTextSmall": false,
    "modal-navbar-override": {
      "button-list": [
        {
          "button-class": "a-modal-close",
          "aria-label": "Lukk",
          "icon-prefix": "ai",
          "icon-class": "ai-exit",
          "icon-extra-class": "a-modal-close-icon",
          "stacked-background": {
            "icon-prefix": "ai",
            "icon-class": "ai-plain-circle-big"
          },
          "is-modal-close": {
            "data-target": "#modal"
          },
          "onclick": "goBack()"
        }
      ]
    }
  },
  "footer": {
    "footer-search": {
      "text-input-id": "text-input-search-11",
      "top-label": "Søk etter innhold",
      "text-input-placeholder": "Søk etter innhold",
      "input-type": "search",
      "isHidden": true,
      "isLarge": false,
      "isInput-group": "input-group",
      "form-input-hasButton": {
        "button-icon": {
          "icon-prefix": "ai",
          "icon-class": "ai-search"
        },
        "button-text": [
          {
            "button-text-value": "Søk",
            "button-text-class": "sr-only"
          }
        ],
        "form-input-button-action": "Utfør søk",
        "onclick": "location.href='link.sider-altinnett-sok-forside';"
      }
    },
    "menuItem1": "Seksjonsforside",
    "urlMenuItem1": "link.sider-altinnett-seksjonsforside"
  }
