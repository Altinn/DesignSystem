{
  "logo" : "../../images/a-logo-white.svg",
  "show-remove-rights": true,
  "show-success-receipt": false,
  "button-list": [
    {
      "button-class": "a-modal-back",
      "aria-label":  "Tilbake",
      "button-icon-class": "material-icons a-modal-back-icon",
      "button-icon-text": "keyboard_backspace",
      "button-onclick": "onclick=\"goBack()\""
    },
    {
      "button-class": "a-modal-close",
      "aria-label":  "Lukk",
      "button-icon-class": "material-icons a-modal-close-icon",
      "button-icon-text": "close"
    }
  ],
  "form-label-hasIcon": {
    "form-label-iconClass": "material-icons",
    "form-label-icon": "&#xE1DB;"
  },
  "form-input-hasButton-class": "a-form-control-withBtn",
  "form-input-hasButton": {
    "form-input-button-action": "Utfør søk",
    "form-input-button-icon": "&#xE8B6;",
    "form-input-button-iconClass": "material-icons"
  },
  "isInput-group": "input-group",
  "form-label-class": "input-group-addon",
  "text-input-label": "<span class=\"sr-only\">Finn klient</span>",
  "text-input-placeholder": "Hvilken klient ser du etter?",
  "input-type": "search",
  "no-left-padding": true,
  "search-algorithm": "show-and-highlight",
  "search-target": "employee-table",
  "table-remove-undo-id": "employee-table",
  "logo-class": "a-modal-top-logo",
  "modal-title": "Gi og fjerne rettigheter",
  "modal-icon": "group_add",
  "input-find-id": "input-find",
  "input-find-icon": "&#xE1DB;",
  "input-find-text": "Hvilken klient ser du etter?",
  "cancel-link": {
    "button-text": "Avbryt",
    "button-class": "a-btn-link a-link a-link-danger pull-right"
  },
  "segment-fixed": "segment-fixed",
  "full-width-table": "full-width-table",
  "ferdigsegment-done": {
    "button-text": "Ferdig",
    "button-class": "a-btn a-btn-success"
  },
  "ferdigsegment-cancel": {
    "button-text": "Avbryt",
    "button-class": "a-btn-link"
  },
  "table-eventhandler" : "tableRowToggle",
  "table-remove-undo-id" : "fjern-angre",
  "sortable-row-actions": [
    {
      "sortable-row-text": "Fjern",
      "sortable-row-icon-type": "remove_circle",
      "sortable-row-icon-class": "a-table-icon remove-icon",
      "sortable-row-action-type": "remove-action"
    },
    {
      "sortable-row-text": "Angre",
      "sortable-row-icon-type": "history",
      "sortable-row-icon-class": "a-table-icon undo-icon red",
      "sortable-row-action-type": "undo-action",
      "hidden": true
    },
    {
      "sortable-row-text": "Slettet",
      "sortable-row-icon-type": "",
      "sortable-row-icon-class": "",
      "hidden": true,
      "sortable-row-action-type": "deleted-action"
    }
  ],
  "sortable-has-head" : {
    "sortable-head-items" : [
      {
        "sortable-head-text": "",
        "sortable" : false,
        "is-first-column": true,
        "cell-padding": true
      },
      {
        "sortable-head-text": "Selskapsnavn",
        "sortable" : true
      },
      {
        "sortable-head-text": "Org.nr",
        "sortable" : true
      },
      {
        "sortable-head-text": "Fjern alle klienter ",
        "sortable-head-icon-type": "remove_circle",
        "sortable" : false,
        "cell-padding": true,
        "last-column": true
      }
    ]
  }
}
