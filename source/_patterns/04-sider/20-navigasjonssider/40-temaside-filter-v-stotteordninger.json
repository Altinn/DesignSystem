{
	"temaside-stotteordninger": {
		"page-title": "Støtteordninger",

		"article-breadcrumb": {
			"breadcrumb-item": [
				{
					"breadcrumb-title": "Starte og drive bedrift",
					"breadcrumb-class": "breadcrumb-mobile",
					"breadcrumb-link": "link.sider-seksjonsforside-v-starteogdrive"
				},
				{
					"breadcrumb-title": "Støtteordninger",
					"breadcrumb-class": "active"
				}
			]
		},

		"hasIntro": {
			"lead-text-id": "leadText",
		  "lead-text-content": "Som næringsdrivende tilbyr det offentlige en rekke støtteordninger. Vi har samlet en oversikt over disse. Hvis du er usikker på hvilken som passer for deg kan du kontakte gründertelefonen hos Innovasjon Norge. "
		},

		"wrapperClass": "a-js-genericSearch",
		"wrapperSource": "../../data/stotteordninger.json,http://altinn-dev.dev.bouvet.no/api/getsubsidy",
		"wrapperDimensions": "Purposes,Industries",
		"wrapperDimensionsAliases": "Purpose,Industry",
		"wrapperMappedKeys": "NAME=SubsidyName,DESC=SubsidyIntro,URL=SubsidyURL,ID=FilterId,TITLE=FilterName",
		"resultList": "a-js-results",
		"alternativeResultList": "a-js-alternativeResults",

		"modal-loader": true,
		"modal-empty": {
			"paragraph-class": "a-js-noResults",
			"paragraph-content": "Ingen treff. Prøv et annet filter."
		},

		"expand-results": {
			"button-text": "Vis aktuelle støtteordninger for meg",
			"button-class": "a-btn a-btn--full a-js-expandResults",
		  "button-colorClass": "",
		  "link-url": "",
		  "button-icon": false
		},

		"filter-formaal": {
			"card-content": false,
			"a-card-filter": "a-card-filter a-js-filterDim1",
			"card-filter": {
				"aria-expanded-header": "false",
				"role": "list",
				"collapse-header-title": "Du har valgt <span class='badge badge-primary a-label badge-pill'></span> formål",
				"collapse-header-title-none": "Velg formål",
			  "data-target": "formaal-filter",
			  "collapse-header-contentID": "formaal-filter",
			  "collapsed": true,
			  "filterable-items": {
			    "checkbuttons": [
			      {
			        "checkbuttons-id": "filterDim1ID%ID%",
			        "checkbuttons-title": "%TITLE%",
			        "hasIcon": false
			      }
			    ]
			  }
			}
		},
		"filter-bransje": {
			"card-content": false,
			"a-card-filter": "a-card-filter a-js-filterDim2",
			"card-filter": {
				"aria-expanded-header": "false",
				"role": "list",
				"collapse-header-title": "Du har valgt <span class='badge badge-primary a-label badge-pill'></span> bransje<span class='a-js-plural'>r</span>",
				"collapse-header-title-none": "Velg bransje",
			  "data-target": "bransje-filter",
			  "collapse-header-contentID": "bransje-filter",
			  "collapsed": true,
			  "filterable-items": {
			    "checkbuttons": [
			      {
			        "checkbuttons-id": "filterDim2ID%ID%",
			        "checkbuttons-title": "%TITLE%",
			        "hasIcon": false
			      }
			    ]
			  },
				"filter-infoText": "* Støtteordninger som er uavhengige av bransje vil også vise i listen."
			}
		},
		"extra-results-heading": "<h2 class='a-h4 a-borderBottom mt-4 mb-2 a-js-result a-js-extraHeading'>Støtteordninger uavhengige av bransje:</h2>",
		"list-stotteordninger": [
			{
				"identifier": "%IDENTIFIER%",
				"heading-start": "<h2>",
			  "heading-end": "</h2>",
			  "linkTitle": {
			    "link-text": "%NAME%",
			    "link-class": "a-link-title",
					"link-url": "link.sider-artikkel-v-stotteordning"

			  },
			  "category-element-description": "%DESC%"
			}
		],
		"list-stotteordninger-loadmore": {
			 "action-button-text": "Last flere",
		   "button-class": "a-btn a-btn-action a-fullWidthBtn a-js-moreResults",
		   "icon-prefix": "ai",
		   "icon-class": "ai-plus",
		   "icon-extra-class": ""
		}
	}
}
