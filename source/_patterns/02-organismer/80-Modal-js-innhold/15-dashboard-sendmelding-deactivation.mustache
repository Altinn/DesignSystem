<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                <span class="a-iconText-text-small"></span>
                <span class="a-iconText-text-large">Bestill endring av informasjon om skjema / tjeneste p√• altinn.no</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">
      {{# tjenesteeier }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ tjenesteeier }}
    
      {{# tjenestenavn }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ tjenestenavn }}

      {{# tjenestetype }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ tjenestetype }}
    
      {{# options1 }}
        <div class="row mb-2">
          <div class="col-8 px-0">
            {{> atomer-avkrysningsboks }}
          </div>
        </div>
      {{/ options1 }}

      {{# environment }}
        <div class="row a-form-group mb-2">
          <div class="col-12 px-0">
            <label class="a-form-label" for="{{ form-id }}">{{ top-label }}</label>
          </div>
          <div class="col-12 px-0">
            <form id={{ form-id }}>
              {{> atomer-valgknapp-ett-alternativ }}
            </form>
          </div>
        </div>
      {{/ environment }}

      {{# tjenestekoder }}
        <div class="row mb-2">
          <div class="col-12 col-lg-5 px-0">
            {{# tjenestekode }}
              {{> atomer-tekst-input}}
            {{/ tjenestekode }}
          </div>
          <div class="col-12 col-lg-6 offset-lg-1 px-0">
            {{# utgavekode }}
              {{> atomer-tekst-input}}
            {{/ utgavekode }}
          </div>
        </div>
      {{/ tjenestekoder }}
      
      {{# tjenesteversjon }}
        <div class="row mb-2">
          <div class="col-5 px-0">
              {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ tjenesteversjon }}

      {{# saneringsdato }}
        <div class="row mb-2">
          <form id={{ form-id }}>
            {{> molekyler-dato-og-tid }}
          </form>
        </div>
      {{/ saneringsdato }}
    
      {{# fjernkopi }}
        <div class="row mb-2">
          <div class="col-12 px-0">
            {{> atomer-avkrysningsboks }}
          </div>
        </div>
      {{/ fjernkopi }}

      {{# premisser }}
        <div class="row mt-2">
          <div class="col-8 px-0">
            {{> atomer-tekstfelt }}
          </div>
        </div>
      {{/ premisser }}

      {{# information }}
        <div class="row mt-2">
          <div class="col-8 px-0">
            {{> atomer-tekstfelt }}
          </div>
        </div>
      {{/ information }}

      {{# submit}}
        <div class="row">
          {{> atomer-knapp:mb-2 }}
        </div>
      {{/ submit}}
    </div>
</div>
