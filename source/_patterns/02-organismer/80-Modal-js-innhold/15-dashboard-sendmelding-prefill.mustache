<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                <span class="a-iconText-text-small"></span>
                <span class="a-iconText-text-large">Bestill innlesing av prefill eller abonnement</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">
      
        {{# tjenesteeier }}
          <div class="row mb-2">
            <div class="col-6 px-0">
              {{> atomer-tekst-input}}
            </div>
          </div>
        {{/ tjenesteeier }}
     
        {{# filetype }}
          <div class="row">
            <form id={{ form-id }}>
              {{> atomer-radioknapp }}
            </form>
          </div>
        {{/ filetype }}
      
      
      {{# in-inbox }}
        <div class="row">
          <form id={{ form-id }}>
            {{> atomer-radioknapp }}
          </form>
        </div>
      {{/ in-inbox }}

      {{# environment }}
        <div class="row">
          <form id={{ form-id }}>
            {{> atomer-radioknapp }}
          </form>
        </div>
      {{/ environment }}

      {{# information }}
        <div class="row mt-2">
          <div class="col-8 px-0">
            {{> atomer-tekstfelt }}
          </div>
        </div>
      {{/ information }}
      
      {{# upload }}
        <div class="row">
          {{> atomer-upload }}
        </div>
      {{/ upload }}

      {{# submit}}
        <div class="row">
          {{> atomer-knapp:mb-2 }}
        </div>
      {{/ submit}}
    </div>
</div>
