<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                <span class="a-iconText-text-small">Saksnummer {{ saksnummer }}</span>
                <span class="a-iconText-text-large">{{ emne }}</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">
      <div class="row">
        <div class="col-12 ml-0">
          {{# message }}
            {{> atomer-tekstfelt }}
          {{/ message }}
        </div>
        <div class="col-12 ml-0">
          {{> molekyler-last-opp-og-fjern }}
        </div>
        <div class="col-12 ml-0 mt-4">

          {{# checkbox }}
            {{> atomer-avkrysningsboks }}
          {{/ checkbox }}

        </div>
      </div> 
      {{#submit1}}
        {{> atomer-knapp:mb-2 }}
      {{/submit1}}
     
    </div>
</div>
