<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                <span class="a-iconText-text-small"></span>
                <span class="a-iconText-text-large">Bestill oppsett av lenketjeneste i tjenesteutviklingsl√∏sningen</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">
      
      {{# serviceowner }}
        <div class="row mb-1">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ serviceowner }}

      {{# servicename }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ servicename }}
      
      {{# url }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ url }}

      {{# conditions1 }}
        <div class="row mb-2">
          <form id={{ form-id }}>
            {{> atomer-avkrysningsboks }}
          </form>
        </div>
      {{/ conditions1 }}
      
      {{# conditions2 }}
        <div class="row">
          <form id={{ form-id }}>
            {{> atomer-radioknapp }}
          </form>
        </div>
      {{/ conditions2 }}

      {{# role }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ role }}

      {{# securitylevel }}
        <div class="row">
          <form id={{ form-id }}>
            {{> atomer-radioknapp }}
          </form>
        </div>
      {{/ securitylevel }}

      {{# test-url }}
        <div class="row mb-2">
          <div class="col-6 px-0">
            {{> atomer-tekst-input}}
          </div>
        </div>
      {{/ test-url }}

      {{# information }}
        <div class="row mt-2">
          <div class="col-8 px-0">
            {{> atomer-tekstfelt }}
          </div>
        </div>
      {{/ information }}

      {{# submit}}
        <div class="row">
          {{> atomer-knapp:mb-2 }}
        </div>
      {{/ submit}}
    </div>
</div>
