<!-- Begin .header -->
<a href="#content" class="sr-only sr-only-focusable">Hopp til hovedinnholdet</a>
<!-- Skip to main content -->

<header class="a-header {{{ header-color }}}" role="banner">
	<!-- Begin .container -->
  <div class="container">
    <!-- Begin Proxy content container, for inclusion in more than one place -->
    <div class="a-js-propagatedContentOrigin droplang">
      {{# dropdown-languages }}
        {{> organismer-meny-nedrulling}}
      {{/ dropdown-languages }}
    </div><!-- End Proxy content container -->

    <script type="text/javascript">
      function langTriggerClick(e) {
        var key = e.which;
        if(key === 13) {
          $(e.target).trigger('mousedown')
        } else if (key === 9) {
          if (!$('#exCollapsingNavbar').find('.a-dropdown-languages').hasClass('expand')) {
            $('#exCollapsingNavbar').find('.a-dropdown-languages').find('a').attr('tabindex', '-1');
          } else {
            $('#exCollapsingNavbar').find('.a-dropdown-languages').find('a').attr('tabindex', '0');
          }
        }
      }
    </script>

		<!-- Begin .a-globalNav-options -->
		<ul class="nav nav-pills a-header-options">

			<li class="nav-item dropdown hidden-xs-down">
        <!-- Dropdown toggle -->
        <a class="nav-link dropdown-toggle a-languageSwitcher" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"> <i class="material-icons hidden-md-up">&#xE894;</i><span class="sr-only">Bytt språk</span> <span class="hidden-sm-down" aria-hidden="true">Language</span></a>
        <span class="propagated-content-destination replace-me" data-refclass="droplang"></span>
      </li>

			<li class="nav-item dropdown">
				<!-- Dropdown toggle -->
				<a class="nav-link a-personSwitcher" data-toggle="{{{ data-toggle-personInfo }}}" href="{{{ person-login-link }}}" role="button" aria-haspopup="true" aria-expanded="false"> <span class="a-personSwitcher-name hidden-sm-down" title="{{login-name}} {{login-represent}}"> {{{ login-name }}} <br> {{{ login-represent }}} </span> <i class="material-icons  a-personSwitcher-icon">&#xE853;</i></a>

				{{# dropdown-personswitch-list }}
					{{> organismer-meny-personbytte }}
				{{/ dropdown-personswitch-list }}
			</li>

		</ul> <!-- End .a-globalNav-options -->


		<!-- Begin .a-globalNav -->
		<nav class="a-globalNav">
			<a href="../../{{ link.sider-forside }}" class="a-globalNav-logo"> {{> atomer-logo }} <span class="sr-only">Til forsiden</span>
      </a>

			  <button class="navbar-toggler hidden-sm-up" type="button" data-toggle="collapse" data-target="#exCollapsingNavbar2">
			  </button>
			  <div class="collapse navbar-toggleable-xs a-globalNav-main" id="exCollapsingNavbar2">
					<ul class="a-dropdownTriangle" id="exCollapsingNavbar">

						<li class="hidden-sm-up">
							{{> atomer-sok }}
						</li>
            <li class="hidden-sm-up"><a class="hvr-underline-from-center"
              onkeyup="langTriggerClick(event)" onmousedown="
              $('#exCollapsingNavbar').find('.a-dropdown-languages').find('a').attr('tabindex', '-1');
              if (!$('#exCollapsingNavbar').find('.a-dropdown-languages').hasClass('expand')) {
                $('#exCollapsingNavbar').find('.a-dropdown-languages').css('width', 'inherit').css('min-width', '160px');
                $('#exCollapsingNavbar').find('.a-dropdown-languages').removeClass('after-collapse');
                $('#exCollapsingNavbar').find('.a-dropdown-languages').find('a').attr('tabindex', '0')
              } else {
                setTimeout(function () {
                  if (!$('#exCollapsingNavbar').find('.a-dropdown-languages').hasClass('expand')) {
                    $('#exCollapsingNavbar').find('.a-dropdown-languages').toggleClass('after-collapse');
                  }
                  if (!$('#exCollapsingNavbar').find('.a-dropdown-languages').hasClass('after-collapse')) {
                    $('#exCollapsingNavbar').find('.a-dropdown-languages').css('width', '0px').css('min-width', '0px');
                  }
                  $('#exCollapsingNavbar').find('.a-dropdown-languages').find('a').attr('tabindex', '-1')
                }, 250)
              }
              $('#exCollapsingNavbar').find('.a-dropdown-languages').toggleClass('expand');
              $('#exCollapsingNavbar').find('.indicator').toggleClass('flip');
              " href="#"><b>language</b><span class="indicator">▼</span></a></li>
            <span class="propagated-content-destination replace-me" data-refclass="droplang"></span>
						<li><a class="hvr-underline-from-center" href="../..{{ link.sider-skjemagruppe }}">innboks</a></li>
						<li><a class="hvr-underline-from-center {{{ globalNav-active }}}" href="../..{{ link.sider-skjema }}">alle skjema</a></li>
						<li><a class="hvr-underline-from-center" href="../..{{ link.sider-om-skjema }}">profil</a></li>
						<li class="hidden-sm-up"><a class="a-nav-link hvr-underline-from-center" href="#">starte og drive bedrift</a></li>
						<li class="hidden-sm-up"><a class="hvr-underline-from-center" href="../..{{ link.sider-artikkel }}">om altinn</a></li>
						<li class="hidden-sm-up"><a class="hvr-underline-from-center" href="../..{{ link.sider-artikkel }}">hjelpesider</a></li>
						<li class="hidden-sm-up">
							{{> atomer-link-telefonnummer }}
						</li>
					</ul>
			  </div>
		</nav> <!-- End .a-globalNav -->

	</div> <!-- End .container -->
</header> <!-- End .header -->
