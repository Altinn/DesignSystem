<div class="row">
  <div class="col-xs-8">
    {{# person-role }} 
      {{> molekyler-person-rolle }} 
    {{/ person-role }}
  </div>
  <div class="col-xs p-t-1">
    {{# cancel-link }} 
      {{> atomer-knapp-link-fare }} 
    {{/ cancel-link }}
  </div>
</div>
<div class="row">
  <div class="col-xs-12 col-md-12 col-lg-5 m-t-3 m-b-1">
    {{> atomer-tekst-input }}
  </div>
</div>
{{# expandable-table-existing-clients }}
  {{# existing-clients-title }} 
    {{> atomer-ekspanderbar-tittel }} 
  {{/ existing-clients-title }}
  <div id={{ collapse-header-contentID }} class="a-collapseContent collapse in disable-animation" role="tabpanel" aria-expanded=" {{ area-expanded }}">
    {{# existing-clients-table }} 
      {{> atomer-tabell-sorterbar }} 
    {{/ existing-clients-table }}
  </div>
{{/ expandable-table-existing-clients }}
{{# expandable-table-new-clients }}
  {{# new-clients-title }} 
    <div class="m-t-2">
      {{> atomer-ekspanderbar-tittel }}
    </div>
  {{/ new-clients-title }}
  <div id={{ collapse-header-contentID }} class="a-collapseContent collapse in disable-animation" role="tabpanel" aria-expanded=" {{ area-expanded }}">
    {{# new-clients-table }} 
      {{> atomer-tabell-sorterbar }} 
    {{/ new-clients-table }}
  </div>
{{/ expandable-table-new-clients }}

