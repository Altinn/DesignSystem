<div class="row">
  <div class="col"></div>
  {{# activitylog }}
    <div class="col-12 col-md-auto mb-1 mb-md-0 text-left text-md-right">
      {{> atomer-link-ikon:a-fontReg|a-fontSizeXS|pr-0 }}
    </div>
  {{/ activitylog }}
  {{# message-info }}
    <div class="col-12 col-md-auto text-left text-md-right">
      <span class="a-fontSizeXS {{ message-status-class }}">{{ message-status }}</span><span class="a-fontSizeXS {{ message-date-class }}">{{ message-date }}</span>
    </div>
  {{/ message-info }}
</div>
{{# forwarded-notice }}
  <div class="row mt-1">
    <div class="col">
      {{> atomer-varsel }}
    </div>
  </div>
{{/ forwarded-notice }}
{{# message-text-block }}
  <div class="row{{# extra-class }} {{ extra-class }}{{/ extra-class }}">
    <div class="col">
      {{# text-paragraph }}
        <p class="my-0{{# text-paragraph-class }} {{ text-paragraph-class }}{{/ text-paragraph-class }}">{{{ message-text }}}</p>
      {{/ text-paragraph }}
      {{# text-link }}
        {{> atomer-link }}
      {{/ text-link }}
    </div>
  </div>
{{/ message-text-block }}
{{# message-list-block }}
  <div class="row{{# extra-class }} {{ extra-class }}{{/ extra-class }}">
    <div class="col-12">
        {{> molekyler-liste-unummerert }}
    </div>
  </div>
{{/ message-list-block }}
{{# attachments-title }}
  <hr>
  <h4 class="a-fontReg">{{ attachments-title }}</h4>
{{/ attachments-title }}
{{# attachments }}
  <div class="row mt-3">
    <div class="col-12">
      {{> atomer-liste-unummerert-ikoner:ai-fontSizeS }}
    </div>
    {{# download-all }}
      <div class="col-12">
        {{> atomer-knapp-ikon-tekst }}
      </div>
    {{/ download-all }}
  </div> 
{{/ attachments }}
{{# info-notice }}
  <div class="row mt-3">
    <div class="col">
      {{> atomer-varsel }}
    </div>
  </div>
{{/ info-notice }}
{{# archive-reference }}
  <div class="row mt-1">
    <div class="col a-fontSizeXXS">
      <span>{{ archive-reference-text }}</span><span>{{ archive-reference-number }}</span>
    </div>
  </div>
{{/ archive-reference }}
{{# expandable-content }}
  <div class="row mt-3">
    <div class="col">
      {{> atomer-handlingsknapp-ekspanderbart-innhold }}
    </div>
  </div>
{{/ expandable-content }}
{{# actions-row }}
  <div class="row">
    <div class="{{ button-wrapper-class}} mt-3">
      {{# action-button }}
        {{> atomer-knapp:mb-0|a-btn-no-textwrap }}
      {{/ action-button }}
    </div>
    <div class="{{ button-icons-wrapper-class }} mt-3">
      {{# action-button-icon }}
        {{> atomer-knapp-ikon-tekst }}
      {{/ action-button-icon }}
    </div>
  </div>
{{/ actions-row }}