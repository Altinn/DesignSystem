<div class="modal-content">
  <div class="modal-header a-modal-header">
    <div class="a-iconText a-iconText-background a-iconText-large">
      <div class="a-iconText-icon">
        <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
      </div>
      <h1 class="a-iconText-text mb-0">
         {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
        <span class="a-iconText-text-large">Bestill installasjon av mappere</span>
      </h1>
    </div>
  </div>
  <div class="modal-body a-modal-body">
    {{ #tab3 }}
    <form data-toggle="validator" id="contactForm2">
      <div class="row">
        <div class="col-12 col-sm-8">

          {{# lenke }}
          {{!-- Obligatorisk --}}
          <div class="mb-2">
              {{> atomer-tekst-input }}
          </div>
          {{/ lenke }}

          {{# environment }}
          {{!-- Må velge minst én --}}
            {{> molekyler-valgknapper-og-spesifiser }}
          {{/ environment }}

          {{# nybruker }}
              <form id="nybruker" class="mb-2">
                {{> atomer-avkrysningsboks }}
              </form>
            
            <div id="bruker" class="collapse mb-2">
              {{# kontakt1 }}
              {{> organismer-dashboard-kontaktinfo }}
              {{/ kontakt1 }}
            </div>

          {{/ nybruker }}

          {{# information }}
              {{> atomer-tekstfelt }}
          {{/ information }}

          {{#submit}}
          {{> atomer-knapp:mb-2 }}
          {{/submit}}

        </div>
      </div>
    </form>
    {{/tab3 }}
  </div>
</div>