<div class="modal-content">
  <div class="modal-header a-modal-header">
      <div class="a-iconText a-iconText-background a-iconText-large">
          <div class="a-iconText-icon">
              <i class="ai ai-send
                a-icon" aria-hidden="true"></i>
          </div>
          <h1 class="a-iconText-text mb-0">
               {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
              <span class="a-iconText-text-large">Bestill endring av grensesnitt</span>
          </h1>
      </div>
  </div>
  <div class="modal-body a-modal-body">
    
  <h2 class="a-h3 mb-2">Innsendingstjeneste over webservice</h2>
  <form data-toggle="validator" id="contactForm">
    {{# existing }}
      <div class="mb-2">
        {{> atomer-avkrysningsboks }}
        <div id="eksisterende" class="collapse">
          {{# name }}
            {{> atomer-tekst-input }}
          {{/ name }}
        </div>
      </div>
    {{/ existing }}

    {{!-- Må velge én --}}
    {{> molekyler-valgknapper-og-spesifiser:mb-2 }}

    {{# version }}
      {{> atomer-tekst-input:mb-4 }}
    {{/ version }}
    {{# attachments }}
      <div class="mb-2">
        {{> atomer-avkrysningsboks }}
        <div id="vedlegg" class="collapse">
          {{# vedlegg }}
            <form id="vedlegg-radio">
            {{> atomer-radioknapp }}
            </form>
          {{/ vedlegg }}
        </div>
      </div>
    {{/ attachments }}
    {{# encoding }}
      <form id="encoding-radio">
        {{> atomer-radioknapp }}
      </form>
    {{/ encoding }}
    {{# sekvensnummer }}
      {{> atomer-tekst-input:mb-4 }}
    {{/ sekvensnummer }}
    {{# standardport }}
      <div class="mb-2">
        {{> atomer-avkrysningsboks }}
        <div id="port" class="collapse">
          {{# port }}
            {{> atomer-tekst-input }}
          {{/ port }}
        </div>
      </div>
    {{/ standardport }}
    {{# transfertype }}
      <form id={{ form-id }} class="mb-2">
        {{> atomer-radioknapp }}
        <div class="radiocollapse collapse no-transition">
          {{> atomer-tekst-input }}
        </div>
      </form>
    {{/ transfertype }}
    {{# operasjonsnavn }}
      <form id={{ form-id }} class="mb-2">
        {{> atomer-radioknapp }}
      </form>
    {{/ operasjonsnavn }}
    {{# soap }}
      <form id={{ form-id }}>
        {{> atomer-radioknapp }}
      </form>
    {{/ soap }}
    {{# url }}
      {{> atomer-tekst-input:mb-2 }}
    {{/ url }}
    {{# autentisering }}
      <form id={{ form-id }} class="mb-2">
        {{> atomer-radioknapp }}
        <div class="collapse passwordcollapse no-transition">
          {{# username }}
            {{> atomer-tekst-input }}
          {{/ username }}
          {{# password }}
            {{> atomer-tekst-input }}
          {{/ password }}
        </div>
        <div class="collapse certificatecollapse no-transition">
          {{# certificate }}
            {{> atomer-tekstfelt }}
            {{> molekyler-last-opp-og-fjern }}
          {{/ certificate }}
        </div>
      </form>
    {{/ autentisering }}
    {{# information }}
      {{> atomer-tekstfelt:mb-2 }}
    {{/ information }}
    {{# submit}}
      {{> atomer-knapp:mb-2 }}
    {{/ submit}}
  </form>
</div>
