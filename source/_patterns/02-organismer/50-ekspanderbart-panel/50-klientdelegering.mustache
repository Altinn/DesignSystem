<div class="panel panel-default a-panel a-collapse" role="tablist">
	{{> molekyler-panel-header }}
	<div id="{{ collapse-panel-id }}" class="panel-collapse a-panel-collapse collapse" role="tabpanel" aria-labelledby="{{ collapse-panel-id }}">
		<div class="panel-collapse-content-section p-x-2 p-b-2">
			<div class="first-action p-t-3">
				{{> atomer-knapp-veksle }}
			</div>
			{{# klientdelegering-file }}
				<div class="third-action p-t-3">
					{{> atomer-knapp }}
				</div>
			{{/ klientdelegering-file }}
			<div class="second-action p-t-3" id="searchForClient" style="display: none;">
				{{# sok-etter-klient }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-klient }}
			</div>
			<div class="second-action p-t-3" id="searchForPerson">
				{{# sok-etter-person }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-person }}
			</div>
			<div class="fourth-action m-t-3" id="searchHiddenForClient">
					{{# klientdelegering-add }} 
						{{> atomer-handlingsknapp }} 
					{{/ klientdelegering-add}}
			</div>
		</div>
		<div>
			<div id="firstTable">
				{{# employee-table }}
					{{> atomer-tabell-actions }}
				{{/ employee-table }}
			</div>
			<div id="secondTable" style="display: none;">
				{{# clients-table }}
					{{> atomer-tabell-actions }}
				{{/ clients-table }}
			</div>
		</div>
	</div>
</div>
