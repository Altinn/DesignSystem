<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{ isExpanded }}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{{ collapseID }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
      {{# collapseHeader }}
        {{> molekyler-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  	<div id="{{ collapseID }}" class="a-collapsePanel-body a-collapsePanel-body--border no-background collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
		<div class="panel-collapse-content-section px-1 px-sm-2 pb-1 pb-sm-2 panelBox">
			<div class="a-second-action pt-2 pt-sm-3 searchPersonOrBusiness" id="searchForClient" style="display: none;">
				{{# sok-etter-klient }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-klient }}
			</div>
			<div class="a-second-action pt-2 pt-sm-3 searchPersonOrBusiness" id="searchForPerson">
				{{# sok-etter-person }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-person }}
			</div>	
			{{# activity-log }}
      <div class="a-third-action  d-lg-block pt-sm-3 activeLog">
        {{> atomer-link }}
      </div>
      {{/ activity-log }}		
      <div class="a-fourth-action mt-2 mt-sm-3 addPersonOrBusiness" id="searchHiddenForClient">
        {{# andremedrettigheter-add }}
          {{> atomer-handlingsknapp-full-bredde }}
        {{/ andremedrettigheter-add}}
			</div>
		</div>
		<div id="rightHolderList" class="rightHolderList" style="">
        {{> molekyler-liste-unummerert-expandable }}
		</div>
	</div>
</div>