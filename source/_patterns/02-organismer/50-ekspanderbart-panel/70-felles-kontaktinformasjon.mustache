<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{isExpanded}}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#{{ collapseID }}" tabindex="0" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{ collapseID }}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
      {{# collapseHeader }}
        {{> molekyler-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  <div id="{{ collapseID }}" class="a-collapsePanel-body collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
    <div class="a-collapsePanel-body-content">
      <div class="row">
        <div class="col-12">
          <h3 class="a-fontBold a-fontSizeM">
            {{ shared-contactinfo-header }}
          </h3>
        </div>
        <div class="col-12 col-md-4">
          {{# shared-contactinfo-first-input }}
            {{> atomer-input-deaktivert }}
          {{/ shared-contactinfo-first-input }}
        </div>
        <div class="col-12 col-md-7">
          {{# shared-contactinfo-second-input }}
            {{> atomer-input-deaktivert }}
          {{/ shared-contactinfo-second-input }}
        </div>
        <div class="col-12 mb-1">
          {{# shared-contactinfo-message }}
            {{> atomer-varsel:mt-0 }}
          {{/ shared-contactinfo-message}}
        </div>
      </div>
      {{# add-contactinfo }}
       <div class="row">
          <div class="col-12 col-md-4">
            {{# add-shared-contactinfo-first-input }}
              {{> atomer-telefonnummer-input }}
            {{/ add-shared-contactinfo-first-input }}
          </div>
          <div class="col-12 col-md-7">
            {{# add-shared-contactinfo-second-input }}
              {{> atomer-epost }}
            {{/ add-shared-contactinfo-second-input }}
          </div>
          <div class="col-1 hidden-sm-down">
            {{# delete-shared-contactinfo-icon }}
              {{> atomer-knapp-ikon-tekst }}
            {{/ delete-shared-contactinfo-icon }}
          </div>
          <div class="col-12 hidden-md-up mb-2">
            {{# delete-shared-contactinfo-link }}
              {{> atomer-knapp-link-fare }}
            {{/ delete-shared-contactinfo-link }}
          </div>
        </div>
      {{/ add-contactinfo }}
      <div class="row">
        <div class="col-12">
          {{# add-contactinfo-row-button }}
            {{> atomer-handlingsknapp }}
          {{/ add-contactinfo-row-button }}
        </div>
      </div>
      <div class="row">
        <div class="col-12 mt-1">
          {{# add-contactinfo-done-button }}
            {{> atomer-knapp-suksess }}
          {{/ add-contactinfo-done-button }}
        </div>
      </div>
    </div>
  </div>
</div>