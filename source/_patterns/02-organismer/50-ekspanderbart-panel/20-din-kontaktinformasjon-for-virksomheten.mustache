<div class="card a-accordion-card a-collapsePanel {{{ isLarge }}} {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{isExpanded}}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#{{ collapseID }}" tabindex="0" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{ collapseID }}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
      {{# collapseHeader }}
        {{> molekyler-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  <div id="{{ collapseID }}" class="a-collapsePanel-body collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
    <div class="a-collapsePanel-body-content">
      {{# my-contact-for }}
        <div class="row">
          <div class="col-12">
            <h3 class="a-fontBold a-fontSizeM">
              {{ header }}
            </h3>
          </div>
          <div class="col-12 col-md-4">
            {{# first-input }}
              {{> atomer-input }}
            {{/ first-input }}
          </div>
          <div class="col-12 col-md-8">
            {{# second-input }}
              {{> atomer-input }}
            {{/ second-input }}
          </div>
        </div>
        <div class="row">
          <div class="col mt-2">
            {{# link }}
              {{> atomer-link }}
            {{/ link }}
          </div>
        </div>
        <div class="row">
          <div class="col mt-3">
            {{# save-btn }}
              {{> atomer-knapp }}
            {{/ save-btn }}
          </div>
        </div>
      {{/ my-contact-for }}

      <!-- Varselinnstillinger -->
      {{# my-alert-settings }}
        <div class="row">
          <div class="col-12">
            <h3 class="a-fontBold a-fontSizeM">
              {{ header }}
            </h3>
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            {{# checkbox }}
              {{> atomer-avkrysningsboks }}
            {{/ checkbox }}
          </div>
        </div>
        <div class="row">
          <div class="col mt-2">
            {{# link }}
              {{> atomer-link }}
            {{/ link }}
          </div>
        </div>
        <div class="row">
          <div class="col mt-3">
            {{# save-btn }}
              {{> atomer-knapp }}
            {{/ save-btn }}
            {{# cancel-btn }}
              {{> atomer-link }}
            {{/ cancel-btn }}
          </div>
        </div>
      {{/ my-alert-settings }}
    </div>  
  </div>
</div>