{
  "collapseID": "myContactInfoFor",
  "dataParent" : "#accordion",
  "collapse-panel-header-id": "myContactInfoForHeader",
  "collapseHeader" : {
    "variation-class" : "a-iconText-shadow a-iconText-large a-collapseBtn collapsed",
    "hasIcon" : {
      "icon-prefix": "ai",
      "icon-class": "ai-client",
      "icon-extra-class" : "a-icon a-icon-circle"
    },
    "hasTextSmall" : false,
    "icontext" : "Din kontaktinformasjon for virksomheten"
  },

  "my-contact-for": {
    "header": "Lorem ipsum",
