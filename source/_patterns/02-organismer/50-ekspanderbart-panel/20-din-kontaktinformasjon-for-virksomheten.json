      "isRequired" : "required",
      "extra-help" : false,
      "isHidden" : "",
      "isLarge" : "",
      "isValidator" : false,
      "isInput-group" : "input-group",
      "form-label-class" : "input-group-addon",
      "form-label-hasIcon" : {
        "icon-prefix": "ai",
        "icon-class" : "ai-closedmessage"
      }
    }
  },
  "second-input-varsel":     {
    "alert-id" : "mail-success",
