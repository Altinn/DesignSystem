<div class="card a-accordion-card a-collapsePanel{{# isLarge }} {{{ isLarge }}}{{/ isLarge }} {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{isExpanded}}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{ collapseID }}">
      {{# collapseHeader }}
        {{> molekyler-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  <div id="{{ collapseID }}" class="a-collapsePanel-body collapse {{{ isVisibleFromStart }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }} role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
    <div class="a-collapsePanel-body-content">
      {{# crr-reservation }} 
      <div class="row">
        <div class="col mb-2">
            {{> atomer-varsel}} 
        </div>
      </div>
      {{/ crr-reservation }}
      <div class="row">
        {{# my-contact-first-header }}
          <div class="col-12">
            <h3 class="a-fontBold a-fontSizeM">
              {{ my-contact-first-header }}
            </h3>
          </div>
        {{/ my-contact-first-header }}
        <div class="col-12 col-md-4">
          {{# my-contact-first-input }}
            {{> atomer-tekst-input }}
          {{/ my-contact-first-input }}
        </div>
        <div class="col-12 col-md-8">
          {{# my-contact-second-input }}
            {{> atomer-tekst-input }}
          {{/ my-contact-second-input }}
        </div>
        <div class="col-12 mb-1">
          {{# my-contact-first-message }}
            {{> atomer-varsel:mt-0 }}
          {{/ my-contact-first-message}}
        </div>
      </div>
      {{# my-contact-checkbox }}
        <div class="row">
          <div class="col">
              {{> atomer-avkrysningsboks }}
          </div>
        </div>
      {{/ my-contact-checkbox }}
      <div class="row">
        <div class="col-12 mt-1">
          {{# my-contact-third-input }}
            {{> atomer-tekst-input }}
          {{/ my-contact-third-input }}
        </div>
        <div class="col-12 mb-1">
          {{# my-contact-second-message }}
            {{> atomer-varsel:mt-0 }}
          {{/ my-contact-second-message}}
        </div>
      </div>
      <div class="row mt-1">
        <div class="col">
          {{# crr-save-btn }}
            {{> atomer-knapp }}
          {{/ crr-save-btn }}
        </div>
      </div>
    </div>
  </div>
</div>
