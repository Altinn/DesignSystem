<!-- Begin #a-dropdown-personswitchList - -->
<div class="dropdown-menu a-dropdown-personswitchList a-dropdownTriangle">

  <div class="pt-5 pl-3 pr-3">

    {{> atomer-link(link-class: "a-link-logout", link-text: "Logg ut", link-url: "/patterns/04-sider-01-forside-00-forside/04-sider-01-forside-00-forside.html") }}

    {{# personswitch-current }}
      <div class="row">
        <div class="col-sm-12 mb-2">
          {{> molekyler-person-privat }}
        </div>
      </div>
    {{/ personswitch-current }}

    <p class="a-h4">
        Alle dine profiler:
    </p>

    {{# personswitch-search }}
      {{> atomer-tekst-input }}
    {{/ personswitch-search }}

  </div>

  <div class="pt-1 pl-1 pr-1">

    {{# personswitch-persons }}
      {{> molekyler-ekspanderbar-liste-status }}
    {{/ personswitch-persons }}

    {{# loadmore }}
      {{> atomer-laster-inn }}
      {{> atomer-handlingsknapp }}
    {{/ loadmore }}

  </div>

  {{# personswitch-options }}
    {{> atomer-avkrysningsboks }}
  {{/ personswitch-options }}

  {{# personswitch-footer }}
    {{> atomer-link-hjelpefunksjon }}
  {{/ personswitch-footer }}

</div>
