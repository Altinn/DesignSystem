<div class="modal-body a-modal-body">
  {{# modal-kvittering-content-header }}
    <h2 class="mb-3">{{ modal-kvittering-content-header }}</h2>
  {{/ modal-kvittering-content-header }}

  <div class="a-modal-receiptContent mb-3">
    <p>
      {{{ modal-kvittering-content }}}
    </p>
    {{# modal-kvittering-list }}
      <ul>
        {{# modal-kvittering-list-items }}
          <li>{{ modal-kvittering-list-item }}</li>
        {{/ modal-kvittering-list-items }}
      </ul>
    {{/ modal-kvittering-list }}

    {{# modal-kvittering-second-content }}
      <p>
        {{{ modal-kvittering-second-content }}}
      </p>
    {{/ modal-kvittering-second-content }}
  </div>

  {{# links }}
    {{> atomer-link:mr-1 }}
  {{/ links }}
</div>