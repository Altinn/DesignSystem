<div class="a-page">
  {{# header }}
    {{> organismer-header }}
  {{/ header }}

  <section id="content" tabindex="-1">

  	<h1 class="sr-only">Altinn - enklere dialog med det offentlige</h1>

    {{> molekyler-jumbotron }}

    <div class="container">

  		{{# information }}
  			{{> atomer-varsel }}
  		{{/ information }}

      <div class="row mb-3">
        <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
          {{# article-search }}
            {{> atomer-tekst-input }}
          {{/ article-search }}
        </div>
      </div>

      <div class="row mb-3">
        <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
          <h2 class="a-h3">
            {{{ list-header }}}
          </h2>

          {{# list-last-used-forms }}
            {{> molekyler-liste-unummerert:mb-2 }}
          {{/ list-last-used-forms }}

          {{#showAllForms}}
            {{> atomer-link}}
          {{/showAllForms}}
        </div>
      </div>

      <div class="row pt-4 p0-print">

        <div class="col-10 offset-1 col-sm-6 offset-sm-0 col-lg-4 offset-lg-2">
          {{# mediaBlock1 }}
            {{> molekyler-bilde-innhold-under }}
          {{/ mediaBlock1 }}
        </div>

        <div class="col-10 offset-1 col-sm-6 offset-sm-0 col-lg-4">
          {{# mediaBlock2 }}
            {{> molekyler-bilde-innhold-under }}
          {{/ mediaBlock2 }}
        </div>
      </div>

      <div class="row py-4">
        <div class="col-lg-8 offset-lg-2 ">
  				{{# newslist }}
            {{> organismer-artikkelliste }}
          {{/ newslist }}
        </div>
      </div>
  	</div><!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>

{{> molekyler-hjelp-container }}
