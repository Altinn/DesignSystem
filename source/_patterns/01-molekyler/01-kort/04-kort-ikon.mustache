<div class="a-card a-panelIconLeft {{ styleModifier }}">
  {{#card-content}}
    <i class="material-icons">{{{ card-icon }}}</i>

    <div class="a-panelIconLeft-body">
      {{#isIntroText}}
      <span class="a-panelIconLeft-introText">
         {{{ card-introText }}}
        </span>
      {{/isIntroText}}
      {{> atomer-link:a-link }}
      <span class="a-panelIconLeft-code">{{card-code}}</span>
    </div>
  {{/card-content}}
</div>
