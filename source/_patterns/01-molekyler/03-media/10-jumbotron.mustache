<div class="jumbotron jumbotron-fluid a-jumbotron {{{ jumbotron-styles }}}" style="background-color: {{{ jumbotron-bgcolor }}}; background-image: {{{ jumbotron-image-url }}}">
  <div class="container">
    <div class="row">
      <div class="{{ jumbo-md-class }} {{ jumbo-md-offset-class }} {{ jumbo-lg-class }} {{ jumbo-lg-offset-class }} {{ jumbo-xl-class }} {{ jumbo-xl-offset-class }}">

        {{# articlestart }}
      		{{> organismer-artikkel-start }}
      	{{/ articlestart }}

        {{# pagestart }}
          {{> atomer-sidetittel }}
          <span class="a-jumbotron-ingress">{{{ jumbo-intro }}}</span>
          {{# front-main-button }}
            {{> atomer-knapp }}
          {{/ front-main-button }}
          {{# jumbo-search }}
            {{> atomer-tekst-input }}
          {{/ jumbo-search }}
          <span class="a-jumbotron-btn-textright a-h1">
            {{{ front-main-button-text-right }}}
          </span>
          {{# emergency }}
      			{{> atomer-varsel }}
      		{{/ emergency }}
      		{{# warning }}
      			{{> atomer-varsel }}
      		{{/ warning }}
      	{{/ pagestart }}

        {{# jumbo-list }}
          <span class="a-jumbotron-ingress">{{{ jumbo-intro }}}</span>
      		{{> molekyler-liste-unummerert-ny:a-list-large|pb-3 }}
      	{{/ jumbo-list }}

        {{# jumbo-link }}
          {{> atomer-link:a-link-large }}
        {{/ jumbo-link }}
      </div>
    </div>
  </div>
</div>
