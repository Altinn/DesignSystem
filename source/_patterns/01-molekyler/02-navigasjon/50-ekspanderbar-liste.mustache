{{# expandable-list }}
<div class="a-list-heading">
  {{> atomer-ekspanderbar-tittel-med-underlinje }}

  {{# heading-action }}
  <div class="a-list-heading-actions">
    {{ heading-text }}
    <i class="material-icons {{ list-heading-icon-class }}">
        {{{ list-heading-icon-type }}}
    </i>
  </div>
  {{/ heading-action }}
</div>
  <div id="{{ collapse-header-contentID }}" class="a-collapseContent a-js-instantCollapse collapse" aria-expanded="{{ aria-expanded }}">
    {{> atomer-liste-unummerert-rolle }}

    {{# has-link }}
      <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-5 mt-2 mb-1">
          {{> atomer-link-selvstendig-sidebytte }}
        </div>
      </div>
    {{/ has-link }}
  </div>
{{/ expandable-list }}