<ul class="nav a-tabs" role="tablist" {{# tabs-id }}id="{{ tabs-id }}"{{/ tabs-id }}>
  {{# tab-items }}
    <li class="nav-item a-tab">
      <a class="a-tab-btn {{# tab-active }} active {{/ tab-active }}" data-toggle="tab" href="#{{ tab-href }}" role="tab">
        <i class="material-icons a-tab-btn-icon hidden-xs-down">{{ tab-icon }}</i>
        {{ tab-title }}
      </a>
    </li>
  {{/tab-items}}
</ul>
