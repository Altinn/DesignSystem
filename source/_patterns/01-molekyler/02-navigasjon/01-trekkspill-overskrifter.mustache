<div class="a-accordion-large" id="accordion" role="tablist" aria-multiselectable="true">
  
  {{# accordion-header-items }}
    <div class="card">

      <!-- TITLE -->
      <div role="tab" id="{{collapseTitle-id}}">
        {{> atomer-ekspanderbar-overskrift }}
      </div>

      <!-- CONTENT -->
      <div id={{collapse-header-id}} class="a-collapseContent collapse" role="tabpanel" aria-labelledby="{{collapseTitle-id}}">
        <div class="a-collapseContent-inside">
          {{ collapse-header-content }}
          <div class="row">
            <div class="col-md-12 col-lg-7">
              {{# collapse-panel-list }}
                {{> atomer-liste-unummerert:a-item-list-large|pb-2 }}
              {{/ collapse-panel-list }}
              {{# collapse-panel-link }}
                {{> atomer-link:a-link-large }}
              {{/ collapse-panel-link }}
            </div>
            <div class="col-lg-4 offset-lg-1 col-xl-3 offset-xl-2 hidden-md-down a-collapseContent-img">
              {{# collapse-panel-img }}
                {{> atomer-placeholder }}
              {{/ collapse-panel-img }}
            </div>
          </div>
        </div>
      </div>
    </div>
  {{/ accordion-header-items }}
</div>
