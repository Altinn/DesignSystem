<div class="a-accordion-large {{ styleModifier }}" id="accordion{{ id }}" role="tablist" aria-multiselectable="true">

  {{# accordion-header-items }}
    <div class="card">

      <!-- TITLE -->
      <div {{# collapseTitle-id }} id="{{ collapseTitle-id }}" {{/ collapseTitle-id }}>
        {{> atomer-ekspanderbar-overskrift }}
      </div>

      <!-- CONTENT -->
      <div {{# collapse-header-id }} id="{{ collapse-header-id }}" {{/ collapse-header-id }} class="a-collapseContent collapse" data-parent="#{{data-parent}}" role="tabpanel" {{# aria-labelledby }} aria-labelledby="{{ collapseTitle-id }}" {{/ aria-labelledby }} >
        <div class="a-collapseContent-inside{{# collapseContent-inside-extraclass }} {{ collapseContent-inside-extraclass }}{{/ collapseContent-inside-extraclass }}">
          {{# collapse-header-content }}
            {{ collapse-header-content }}
          {{/ collapse-header-content }}

          {{# paragraphContainers }}
            <div class="mb-4">
              {{# paragraphHeader}}
                <h3 class="a-h4 a-fontSizeXS mb-0">{{ paragraphHeader }}</h3>
              {{/ paragraphHeader}}
              {{# paragraph }}
                <p>{{ paragraph }}</p>
              {{/ paragraph }}
              {{# link }}
                <a href="#">{{ linkText }}</a>
              {{/ link }}
            </div>
          {{/ paragraphContainers }}

          {{# readMoreLink }}
            <span>{{ readMoreText }}<a href="#" class="ml-1">{{ readMoreLink }}</a></span>
          {{/ readMoreLink }}

          {{# collapse-header-buttons }}
            <div class="a-btn-group mt-2">
              {{# link-group-items }}
                {{> atomer-link }}
              {{/ link-group-items }}
            </div>
          {{/ collapse-header-buttons }}

          {{# hasRadiogroup-date }}
            {{> molekyler-valgknapper-og-tekstfelt }}
          {{/ hasRadiogroup-date }}

          {{# hasCheckboxes }}
            {{> atomer-avkrysningsboks }}
          {{/ hasCheckboxes }}

          {{# hasNestedCheckboxes }}
            {{> molekyler-nestede-avkrysningsbokser }}
          {{/ hasNestedCheckboxes }}

          {{# hasAvkrysningsbokserOgSok }}
            {{> molekyler-avkrysningsbokser-og-sok }}
          {{/ hasAvkrysningsbokserOgSok }}

          {{# profileSelection }}
            {{> organismer-select-profiles }}
          {{/ profileSelection }}

          {{# listLinkAndImage-content }}
          <div class="row">
            <div class="col-md-12 col-lg-7">
              {{# collapse-panel-list }}
                {{> molekyler-liste-unummerert:a-list-large|pb-2 }}
              {{/ collapse-panel-list }}
              {{# collapse-panel-link }}
                {{> atomer-link:a-link-large }}
              {{/ collapse-panel-link }}
            </div>
            <div class="col-lg-4 offset-lg-1 col-xl-3 offset-xl-2 d-none d-lg-block a-collapseContent-img">
              {{# collapse-panel-img }}
                {{> atomer-placeholder }}
              {{/ collapse-panel-img }}
            </div>
          </div>
          {{/ listLinkAndImage-content }}

          {{# hasNumberedList }}
              <div class="mt-1">
                  {{> atomer-liste-nummerert-sirkel }}
              </div>
          {{/ hasNumberedList }}

          {{# hasUnnumberedList }}
              <div class="mt-1">
                  {{> molekyler-liste-unummerert-lenke }}
              </div>
          {{/ hasUnnumberedList }}
        </div>
      </div>
    </div>
  {{/ accordion-header-items }}
</div>
