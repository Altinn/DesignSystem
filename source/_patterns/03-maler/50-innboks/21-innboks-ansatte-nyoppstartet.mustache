<div class="a-page" id="smoothState">
	{{> organismer-header }}

		<div class="container" id="content" tabindex="-1">

			<div class="row m-t-3 m-b-3">
				<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }} 

				</div>
			</div>

			{{> molekyler-toppoppgaver }}


			<div class="row">
				<div class="col-sm-12 col-lg-12 m-t-4  a-p-x9-xl">
					{{# first-page-title }}
						{{> atomer-sidetittel:a-textSize2|a-textSizeReg }}
					{{/ first-page-title }}

					<p>
						Du kan <strong>ikke</strong> være ansatt i ditt eget enkeltpersonforetak. Men idet du ansetter andre skal disse registrerer og du må sende inn a-melding månedlig. <a class="a-linkArea a-link" href="#">Du kan sende inn via lønnssystem</a> hvis du bruker det. Hvis ikke kan du sende inn a-melding manuelt her:
					</p>
				</div><!-- END .col-... -->
			</div><!-- END .row -->


			<div class="row">
				<div class="col-sm-12 col-lg-12 m-b-1">

					{{# amelding }}
						{{> molekyler-ikon-link }}
					{{/ amelding }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->


			<div class="row">
				<div class="col-sm-12 col-lg-12  a-p-x9-xl">

					 {{# third-page-title }}
						{{> atomer-sidetittel:a-textSize2|a-textSizeReg|m-t-4 }}
					 {{/ third-page-title }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 m-b-1">

					{{# skattekort }}
						{{> molekyler-ikon-link }}
					{{/ skattekort }}


				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12  a-p-x9-xl">

					 {{# second-page-title }}
					 	{{> atomer-sidetittel:a-textSize2|a-textSizeReg|m-t-4 }}
					 {{/ second-page-title }}

					 <p>
 						Det er per i dag <strong>ingen som er sykemeldt i foretaket.</strong> Her vises aktive sykemeldinger for dine ansatte. Hos nav finner du utfyllende <a class="a-linkArea a-link" href="#">informasjon om sykemeldinger og hvilke plikter du har som arbeidsgiver</a>.
 					</p>
				</div><!-- END .col-... -->
			</div><!-- END .row -->




		{{> organismer-footer }}

	</div> <!-- END .container -->

</div> <!-- END .a-page #smoothstate -->
