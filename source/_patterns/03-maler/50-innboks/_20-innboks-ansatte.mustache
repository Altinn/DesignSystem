<div class="a-page" id="smoothState">
	{{> organismer-header }}

		<div class="container" id="content" tabindex="-1">

			<div class="row mt-3 mb-3">
				<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }}
				</div>
			</div>

			{{> molekyler-toppoppgaver }}

			<div class="row">
				<div class="col-sm-12 col-lg-12 mt-4  a-p-x9-xl">
					{{# first-page-title }}
						{{> atomer-sidetittel:a-headingSizeXL|a-fontReg }}
					{{/ first-page-title }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 mb-1">
					{{# messages }}
						{{> organismer-ekspanderbart-panel-trekkspill:a-inbox-messageList }}
					{{/ messages }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 a-p-x9-xl">
					 {{> atomer-link-selvstendig }}
					 {{# second-page-title }}
					 	{{> atomer-sidetittel:a-headingSizeXL|a-fontReg|mt-4 }}
					 {{/ second-page-title }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 mb-2">
					{{# first-group-buttons }}
						{{> molekyler-grupperte-fremhevede-knapper }}
					{{/ first-group-buttons }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 a-p-x9-xl">
					{{> atomer-link-selvstendig }}
					 {{# third-page-title }}
						{{> atomer-sidetittel:a-headingSizeXL|a-fontReg|mt-4 }}
					 {{/ third-page-title }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12">
					{{# second-group-buttons }}
						{{> molekyler-grupperte-fremhevede-knapper }}
					{{/ second-group-buttons }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

		{{> organismer-footer }}

	</div> <!-- END .container -->

</div> <!-- END .a-page #smoothstate -->
