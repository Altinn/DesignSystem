<div class="a-modal fade modal show {{ bodyClass }}" {{# modal-id }} id="{{ modal-id }}" {{/ modal-id }} role="dialog" {{# modal-label }} aria-labelledby="{{ modal-label }}" {{/ modal-label }} aria-hidden="false" style="display: block">
  <div class="modal-dialog modal-lg" role="document">
    <div class="container">
      <div class="row">
        <div class="col-xl-12">
          {{> molekyler-modal-header }}
        </div>
      </div>
      <div class="row">
        <div class="col-xl-10 offset-xl-1">
          {{# modal-navbar-override }}
            {{> molekyler-modal-navbar }}
          {{/ modal-navbar-override }}
          <div class="a-modal-content-target">
            <div class="a-page a-current-page">
              <div class="modalPage">
                <div class="modal-content">
                  {{> molekyler-modal-body-header }}
                  {{# show-client-delegation-content }}
                    {{> organismer-modal-klientdelegering }}
                  {{/ show-client-delegation-content }}
                  {{# show-others-with-rights-content }}
                    {{# others-with-rights }}
                      {{> organismer-modal-andre-med-rettigheter }}
                    {{/ others-with-rights }}
                  {{/ show-others-with-rights-content }}
                  {{# show-receipt-content }}
                    {{> organismer-modal-kvittering }}
                  {{/ show-receipt-content }}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
