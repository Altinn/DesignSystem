<div class="a-page" id="smoothState">

	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1">
					{{# articlestart }}
						{{> molekyler-smulesti }}
						{{> atomer-sidetittel }}
						{{> atomer-byline }}
						{{> atomer-intro-tekst }}
					{{/ articlestart }}
				</div>
			</div>

			<div class="row">
				<div class="col-sm-12 col-lg-8 offset-lg-2">
					<form>
						{{# 1 }}
						{{> atomer-avsnitt }}
						{{/ 1 }}

					{{# 2 }}
						{{> atomer-seksjonstittel }}
						{{> atomer-avsnitt }}
					{{/ 2 }}

						{{# modal-input-keyword }}
							{{> atomer-tekst-input }}
						{{/ modal-input-keyword }}

						{{# modal-loader }}
							{{> atomer-laster-inn }}
						{{/ modal-loader }}


						{{# modal-empty }}
							{{> atomer-avsnitt }}
						{{/ modal-empty }}

					{{# resultList }}
						{{> molekyler-liste-unummerert-ny }}
					{{/ resultList }}

					</form>
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			{{# hasAccordion }}
			<div class="row a-text">
				<div class="col-sm-12 col-lg-8 offset-lg-2 pb-2">
						<h2>{{{ header-2 }}}</h2>
						{{> molekyler-trekkspill }}
				</div>
			</div>
			{{/ hasAccordion }}

			<div class="row">
				<div class="col-sm-6 col-lg-5 offset-lg-2 py-2">
					<h2>{{{ header-3 }}}</h2>
					{{# help }}
						{{{ help-prefix }}}
						{{> atomer-link:a-fontBold }}
						{{{ help-end }}}
					{{/ help }}
				</div>
			</div>

		</div> <!-- END Container -->

		{{> organismer-footer }}

</div>
