<div class="a-page" id="smoothState">
	{{> organismer-header }}

	{{# article-jumbotron }}
		{{> molekyler-jumbotron }}
	{{/ article-jumbotron }}

	<div class="container" id="content" tabindex="-1">
		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text mb-5">
					{{> atomer-avsnitt }}

					{{> atomer-seksjonstittel }}
					{{> atomer-avsnitt }}

					{{# didYouFindWhatYouWereLookingFor }}
					{{> atomer-radioknapp:mt-2 }}
					{{/ didYouFindWhatYouWereLookingFor }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 mb-5">

				{{> molekyler-bilde-innhold }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 pt-3 mb-5">
				{{# article-search }}
					{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
		{{> organismer-footer }}

		</div> <!-- END .container -->

	</div>

</div> <!-- END .a-page #smoothstate -->
