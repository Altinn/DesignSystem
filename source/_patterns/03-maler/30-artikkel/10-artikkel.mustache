<div class="a-page" id="smoothState">
	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1">
				{{# articlestart }}
				{{> organismer-artikkel-start }}
				{{/ articlestart }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text epi-wysiwyg pt-4">
				{{> atomer-avsnitt }}

				{{> atomer-seksjonstittel }}
				{{# cardright }}
					{{> molekyler-ikon-link:a-article-right-off|mb-2 }}
				{{/ cardright }}
				{{> atomer-avsnitt }}

				{{> molekyler-bilde-sitat }}

				{{> atomer-seksjonstittel }}
				{{> atomer-avsnitt }}

				{{> atomer-liste-unummerert-sammenkoblet }}

				{{> atomer-undertittel }}
				{{> atomer-avsnitt }}

				{{> atomer-seksjonstittel }}
				{{> atomer-avsnitt }}
				{{> atomer-liste-unummerert-kuler }}

				{{# cardFirst }}
					{{> molekyler-ikon-link:a-iconText-minusBothMargins }}
				{{/ cardFirst }}

				{{> atomer-seksjonstittel }}
				{{> atomer-avsnitt }}
				{{> atomer-liste-nummerert-sirkel }}

				{{> atomer-sitat:a-article-right-off|a-blockquote-right }}

				{{> atomer-undertittel }}
				{{> atomer-avsnitt }}

				{{> atomer-seksjonstittel }}
				{{> atomer-avsnitt }}

				{{> atomer-undertittel }}
				{{> atomer-bilde:a-articleImage-right }}
				{{> atomer-avsnitt }}

				{{> organismer-artikkel-liste }}

				{{> organismer-gjennomgang }}

				{{# article-compare-first }}
					{{> organismer-sammenligning }}
				{{/ article-compare-first }}

				{{# article-compare-second }}
					{{> organismer-sammenligning }}
				{{/ article-compare-second }}

				{{> atomer-avsnitt }}

				{{# firstlink }}
					<strong>
						{{> atomer-link }}
					</strong>
				{{/ firstlink }}
				<br><br>

				{{> atomer-avsnitt }}

				{{# secondlink }}
					<strong>
						{{> atomer-link }}
					</strong>
				{{/ secondlink }}
				<br><br>

				{{> atomer-avsnitt }}

				<p>Dette er en tekst der et av ordene har en ordforklaring {{> atomer-ordforklaring }} bak seg. Og sÃ¥ kommer resten av teksten i avsnittet..</p>

				{{> atomer-tabell-regnestykke }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text epi-wysiwyg my-4">

				{{> molekyler-fant-du-det-du-lette-etter }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		{{# cardbottom }}
		<div class="row">
			<div class="col-sm-12 col-lg-12 mb-4">
					{{> molekyler-kort }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ cardbottom }}

		{{# listbottom }}
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 mb-5">
					{{> atomer-seksjonstittel:a-h3 }}
					{{> molekyler-liste-unummerert-ny }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ listbottom }}


		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 mt-5 mb-5">
					{{> molekyler-bilde-innhold }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->

	</div> <!-- END Container -->

	{{> organismer-footer }}

</div>
