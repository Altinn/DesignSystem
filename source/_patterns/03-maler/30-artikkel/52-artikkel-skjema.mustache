<div class="a-page" id="smoothState">

	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 p-y-3">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }}
				</div>
			</div>
			{{> molekyler-kort-hoved }}
			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 p-y-3">
					<h2>{{{ header-2 }}}</h2>
					{{> molekyler-trekkspill }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6 col-lg-5 offset-lg-1 p-y-3">
					<h2>{{{ header-3 }}}</h2>
					{{# help }}
						{{{ help-prefix }}}
						{{> atomer-link }}
					{{/ help }}
				</div>
				<div class="col-sm-6 col-lg-5 offset-lg-0 p-y-3">
					<h2>{{{ header-4 }}}</h2>
					{{> atomer-link-telefonnummer:d-block|p-t-1 }}
					{{> atomer-link-mail:d-block|p-t-1 }}
				</div>
			</div>

		{{> organismer-footer }}
	</div> <!-- END Container -->

</div>
