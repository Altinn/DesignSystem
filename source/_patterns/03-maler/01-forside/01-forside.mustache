<div class="a-page" id="smoothState">

	{{# header }}
    {{> organismer-header }}
  {{/ header }}

	<h1 class="sr-only">Altinn - enklere dialog med det offentlige</h1>

  {{> molekyler-jumbotron }}

  <div class="container" tabindex="-1">

		{{# information }}
			{{> atomer-varsel }}
		{{/ information }}

    <div class="row mt-4 mb-3">
      <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
        {{# article-search }}
          {{> atomer-tekst-input }}
        {{/ article-search }}
      </div>
    </div>

    <div class="row mb-3">
      <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
        <h2 class="a-h3">
          {{{ list-header }}}
        </h2>

        {{# list-last-used-forms }}
          {{> molekyler-liste-unummerert }}
        {{/ list-last-used-forms }}
      </div>
    </div>

    <div class="row pt-4" id="content">

      <div class="col-sm-6 col-lg-4 offset-lg-2">
        {{# mediaBlock1 }}
          {{> molekyler-bilde-innhold-under }}
        {{/ mediaBlock1 }}
      </div>

      <div class="col-sm-6 col-lg-4">
        {{# mediaBlock2 }}
          {{> molekyler-bilde-innhold-under }}
        {{/ mediaBlock2 }}
      </div>
    </div>

    <div class="row py-4">
      <div class="col-lg-8 offset-lg-2 ">
				{{# newslist }}
          {{> organismer-artikkelliste }}
        {{/ newslist }}
      </div>
    </div>
	</div>
	{{> organismer-footer }}
</div>
