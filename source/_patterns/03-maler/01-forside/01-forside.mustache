<div class="a-page" id="smoothState">

	{{> organismer-header }}

		{{> molekyler-jumbotron }}

		<div class="container" tabindex="-1">

		<div class="row mt-4 mb-3">
			<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
				{{# article-search }}
					{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>

		<div class="row mb-5">
			<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
				<h2 class="a-textSize3">
					{{{ list-header }}}
				</h2>

					{{# list-last-used-forms }}
						{{> atomer-liste-nummerert }}
					{{/ list-last-used-forms }}
				</div>
			</div>
			<div class="row p-y-4" id="content">

				<div class="col-sm-6 col-lg-4 offset-lg-2">
					{{# mediaBlock1 }}
						{{> molekyler-bilde-innhold-under }}
					{{/ mediaBlock1 }}
				</div>

				<div class="col-sm-6 col-lg-4">
					{{# mediaBlock2 }}
						{{> molekyler-bilde-innhold-under }}
					{{/ mediaBlock2 }}
				</div>
			</div>

			<div class="row p-y-4">
				<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
					{{# list-didyouknow }}
						<h2 class="a-textSize3">
							{{{ list-header }}}
						</h2>
						{{> atomer-liste-unummerert }}
					{{/ list-didyouknow }}
				</div>
			</div>

			{{> organismer-footer }}

		</div>

</div>
