<div class="a-page" id="smoothState">
	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 p-t-3 m-b-5">
				{{# article-search }}
					{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1 m-b-4">
				{{# articlestart }}
				{{> organismer-artikkel-start }}
				{{/ articlestart }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12 col-lg-12 m-b-5">

				{{> molekyler-sammenligning-header }}

				{{# first-compare }}
					{{> organismer-sammenligning }}
				{{/ first-compare }}

				{{# second-compare }}
					{{> organismer-sammenligning }}
				{{/ second-compare }}

				{{# third-compare }}
					{{> organismer-sammenligning }}
				{{/ third-compare }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		<div class="row">
			<div class="col-sm-12 col-lg-6 m-b-2">

				{{# first-card-icon }}
						{{> molekyler-ikon-link }}
				{{/ first-card-icon }}

			</div><!-- END .col-... -->

			<div class="col-sm-12 col-lg-6 m-b-2">

				{{# second-card-icon }}
						{{> molekyler-ikon-link }}
				{{/ second-card-icon }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->



		{{> organismer-footer }}

	</div> <!-- END Container -->

</div>
