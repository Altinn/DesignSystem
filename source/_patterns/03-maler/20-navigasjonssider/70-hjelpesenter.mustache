<div class="a-page">
  {{# header }}
    {{> organismer-header }}
  {{/ header }}

  <section id="content" tabindex="-1">

    {{> molekyler-jumbotron }}

    <div class="container">
      <div class="row mt-4 mb-3">
        <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
          {{# article-search }}
            {{> atomer-tekst-input }}
          {{/ article-search }}
        </div>
      </div>

      {{> molekyler-illustrasjonslenke:mt-4|a-dontPrint }}

      {{# help-questions }}
        <div class="row mt-6">
          <div class="col-sm-12 col-lg-10 offset-lg-1">
            {{> atomer-seksjonstittel }}
          </div>
  			</div>
        <div class="row">
            <div class="col-sm-12">
              {{> molekyler-trekkspill-overskrifter:a-accordion-large--offset }}
  			   </div>
        </div>
      {{/ help-questions }}

      {{# kontakt }}
      <div class="row">
        <div class="col-sm-12 col-10 offset-lg-1">
          {{> molekyler-bilde-innhold:mt-5 }}
        </div>
      </div>
      {{/ kontakt }}

      {{# kontaktskjema }}
        {{> organismer-modal }}
      {{/ kontaktskjema }}

  	</div><!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>

{{> molekyler-hjelp-container }}
