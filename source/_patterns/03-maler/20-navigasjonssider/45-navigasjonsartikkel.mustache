<div class="a-page" id="smoothState">
	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>

			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-2">
						{{> atomer-sidetittel }}
						{{# hasIntro}}
							{{> atomer-intro-tekst }}
						{{/ hasIntro}}
				</div>
			</div>

			{{# hasContent }}
				<div class="row">
					<div class="col-sm-12 col-lg-8 offset-lg-2 pb-3">
						{{{wysiwyg-text}}}
					</div>
				</div>
			{{/ hasContent}}

			{{# first-centerlist }}
			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3">
						{{> atomer-seksjonstittel:a-h3 }}
						{{> molekyler-liste-unummerert:clearfix|mb-4 }}
				</div>
			</div>
			{{/ first-centerlist }}

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 pb-3">
					{{> molekyler-bilde-innhold:mb-6 }}

					{{# card-content-2 }}
						{{> molekyler-bilde-innhold:mb-6 }}
					{{/ card-content-2 }}

					{{# card-content-3 }}
						{{> molekyler-bilde-innhold:mb-6 }}
					{{/ card-content-3 }}

					{{# card-content-4 }}
						{{> molekyler-bilde-innhold:mb-6 }}
					{{/ card-content-4 }}
				</div>
			</div>

		</div> <!-- END Container -->

		{{> organismer-footer }}

</div>
