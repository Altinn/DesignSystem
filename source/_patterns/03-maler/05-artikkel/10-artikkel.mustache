<div class="a-page" id="smoothState">
	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 p-t-3 m-b-5">
				{{# article-search }}
					{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1">
				{{# articlestart }}
				{{> organismer-artikkel-start }}
				<hr class="m-b-2">
				{{/ articlestart }}
			</div>
		</div>

		{{# firstpart }}
		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text m-t-2 m-b-4">
					{{# paragraph1 }}
						{{> atomer-avsnitt }}
					{{/ paragraph1 }}

					{{# sectiontitle1 }}
						{{> atomer-seksjonstittel }}
					{{/ sectiontitle1 }}

					{{# paragraph2 }}
						{{> atomer-avsnitt }}
					{{/ paragraph2 }}

					{{# sectiontitle2 }}
						{{> atomer-seksjonstittel }}
					{{/ sectiontitle2 }}

					{{# paragraph3 }}
						{{> atomer-avsnitt }}
					{{/ paragraph3 }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ firstpart }}

		{{# imagequote }}
		<div class="row">
			<div class="col-sm-12 col-lg-12">
					{{> molekyler-bilde-sitat }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ imagequote }}

		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text">

				{{# sectiontitle3 }}
					{{> atomer-seksjonstittel }}
				{{/ sectiontitle3 }}

				{{# paragraph4 }}
					{{> atomer-avsnitt }}
				{{/ paragraph4 }}

				{{# cardright }}
					{{> molekyler-kort-ikon:a-article-right-off }}
				{{/ cardright }}

				{{# paragraph5 }}
					{{> atomer-avsnitt }}
				{{/ paragraph5 }}

				{{# sectiontitle4 }}
					{{> atomer-seksjonstittel }}
				{{/ sectiontitle4 }}

				{{# first-list }}
					{{> atomer-liste-unummerert-kuler }}
				{{/ first-list }}

				{{# paragraph6 }}
					{{> atomer-avsnitt }}
				{{/ paragraph6 }}

				{{# sectiontitle5 }}
					{{> atomer-seksjonstittel }}
				{{/ sectiontitle5 }}

				{{# second-list }}
					{{> atomer-liste-unummerert-kuler }}
				{{/ second-list }}

				{{# paragraph8 }}
					{{> atomer-avsnitt }}
				{{/ paragraph8 }}

				{{# blockquoteright }}
					{{> atomer-sitat:a-article-right-off|a-blockquote-right }}
				{{/ blockquoteright }}

				{{# subtitle1 }}
					{{> atomer-undertittel }}
				{{/ subtitle1 }}

				{{# paragraph9 }}
					{{> atomer-avsnitt }}
				{{/ paragraph9 }}

				{{# sectiontitle6 }}
					{{> atomer-seksjonstittel }}
				{{/ sectiontitle6 }}

				{{# paragraph10 }}
					{{> atomer-avsnitt }}
				{{/ paragraph10 }}

				{{# subtitle2 }}
					{{> atomer-undertittel }}
				{{/ subtitle2 }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		{{# calculation }}
		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text m-b-5">
					{{> atomer-tabell-regnestykke }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ calculation }}

		{{# articlelist }}
		<div class="row">
			<div class="col-sm-12 col-lg-12 m-b-5">
					{{> organismer-artikkel-liste }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ articlelist }}

		<div class="row">
			<div class="col-sm-12 col-lg-8 offset-lg-2 a-text m-b-5">

				{{# paragraph11 }}
					{{> atomer-avsnitt }}
				{{/ paragraph11 }}

				{{# paragraph12 }}
					{{> atomer-avsnitt }}
				{{/ paragraph12 }}

				{{# firstlink }}
					{{> atomer-link }}
				{{/ firstlink }}

				{{# paragraph13 }}
					{{> atomer-avsnitt }}
				{{/ paragraph13 }}

				{{> atomer-radioknapp:m-t-2 }}

			</div><!-- END .col-... -->
		</div><!-- END .row -->

		{{# cardbottom }}
		<div class="row">
			<div class="col-sm-12 col-lg-12 m-b-4">
					{{> molekyler-kort }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ cardbottom }}


		{{# picturecontent }}
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 m-b-5">
					{{> molekyler-bilde-innhold }}
			</div><!-- END .col-... -->
		</div><!-- END .row -->
		{{/ picturecontent }}

		{{> organismer-footer }}

	</div> <!-- END Container -->

</div>
