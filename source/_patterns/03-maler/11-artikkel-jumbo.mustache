<div class="a-page" id="smoothState">
	{{> organismer-header }}

	{{# jumbotron }}
		{{> molekyler-jumbotron }}
	{{/ jumbotron }}

	<div class="container" id="content" tabindex="-1">

			<div class="row">
				<div class="col-sm-12 col-lg-8 offset-lg-2 m-t-2 m-b-4">

						{{> atomer-seksjonstittel(section-title:"Økonomi") }}

						{{> atomer-avsnitt(paragraph-content:"Som eier av et enkeltpersonsforetak står du økonomisk ansvarlig for din virksomhets gjeld og forpliktelser. Dette betyr at du driver virksomheten din for egen regning og risiko. Skattemessig lignes virksomheten din sammen med deg personlig. Dette betyr at nettooverskuddet er skattepliktig sammen med dine andre inntekter, for eksempel dine lønnsinntekter. På samme måte vil nettotapet være fradragsberettiget. Du er ikke forpliktet å investere kapital i selskapet, siden du uansett er personlig ansvarlig, i motsetning til krav om kaptial i aksjeselskap.") }}

						{{> atomer-seksjonstittel(section-title:"Organisasjonsnummer") }}

						{{> atomer-avsnitt(paragraph-content:"Du får tildelt et organisasjonsnummer når ditt foretak blir registrert via Samordna registermelding. Organisasjonsnummeret blir brukt av det offentlige for å samordne informasjon om foretaket.") }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12">

					{{> molekyler-bilde-sitat }}
					
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 m-b-5">

					{{# first-list }}
						{{> atomer-seksjonstittel:a-textSize3 }}
						{{> atomer-liste-unummerert }}
					{{/ first-list }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 m-b-5">

					{{> molekyler-bilde-innhold }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 m-b-5">

					{{> atomer-sok-stor }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->




		{{> organismer-footer }}

	</div> <!-- END .container -->

</div></div> <!-- END #smoothstate -->
