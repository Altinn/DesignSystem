<div class="a-page">
	{{> organismer-header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<section id="content" tabindex="-1">

		<div class="container a-searchResults">
				<div class="row mb-1 mb-md-2">
					<div class="col-12 col-lg-10 offset-lg-1">
						<h1 class="a-h2 a-fontReg"><b>10 av 123 treff</b> p√• skjema og artikler med <b>"MVA"</b></h1>
					</div>
				</div>

				<div class="row">
					<div class="col-12 col-lg-9 col-xl-8 offset-lg-1">
						{{> molekyler-kategorifaner }}
					</div>
				</div>

				<div class="row">
					<div class="col-12 px-0 px-sm-1">
						<form class="a-searchResults-filter d-flex align-items-center">
							<span class="a-legend d-inline-flex">Begrens treffet:
							</span>
							<div class="d-block pl-1">
							{{# filter-search }}
								{{> atomer-valgknapp:d-inline-flex|flex-wrap|a-switch--small }}
							{{/ filter-search }}
							</div>
						</form>
					</div>
				</div>

				{{# hit-form-1 }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-3 {{a-searchResults-lastOfType}}">
							{{> molekyler-ikon-link }}
					</div>
				</div>
				{{/ hit-form-1 }}

				{{# hit-article-1 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> molekyler-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-1 }}

				{{# infoAboutMail }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-0">
							{{> molekyler-bilde-innhold:a-cardImage-border|a-cardImage-largePadding }}
					</div>
				</div>
				{{/ infoAboutMail }}

				{{# hit-article-2 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark {{a-searchResults-lastOfType}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> molekyler-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-2 }}

				{{# hit-form-2 }}
				<div class="row">
					<div class="col-sm-12 col-lg-12 pb-3 {{a-searchResults-lastOfType}}">
							{{> molekyler-ikon-link }}
					</div>
				</div>
				{{/ hit-form-2 }}

				{{# hit-article-3 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark {{a-searchResults-lastOfType}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> molekyler-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-3 }}

				{{# loadmoreSearch }}
				<div class="row">
					<div class="col-sm-12">
							{{> atomer-handlingsknapp }}
					</div>
				</div>
				{{/ loadmoreSearch }}


			</div> <!-- END Container -->

	</section>

	{{> organismer-footer }}
</div>
