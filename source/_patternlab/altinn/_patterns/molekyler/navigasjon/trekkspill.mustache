<div class="a-accordion-small {{styleModifier}}" id="{{# data-parent }}{{{ data-parent }}}{{/ data-parent }}{{^ data-parent }}accordionSmall{{/ data-parent }}" role="tablist"  aria-multiselectable="true">

  {{# accordion-small-items }}
  <div class="card">

    <!-- TITLE -->
    <div role="tab" id="{{collapseTitle-id}}">
        {{> atomer-ekspanderbar-tekst }}
    </div>

    <!-- CONTENT -->
    <div id="{{collapse-filter-id}}" class="a-collapseContent collapse {{ a-text }}" role="tabpanel" aria-labelledby="{{collapseTitle-id}}" {{# data-parent }} data-parent="#{{ data-parent }}" {{/ data-parent }}>
        {{{ collapse-filter-content }}}
        {{# collapse-listInside }}
          {{> felles-komponenter-liste-unummerert }}
        {{/ collapse-listInside }}
    </div>

  </div>
  {{/ accordion-small-items }}

</div>
