<div class="a-modal fade modal show {{ bodyClass }}" {{# modal-id }} id="{{ modal-id }}" {{/ modal-id }} tabindex="-1" role="dialog" {{# modal-label }} aria-labelledby="{{ modal-label }}" {{/ modal-label }} style="display: block">
  <div class="modal-dialog modal-lg" role="document">
    <div class="container a-p-static">
      <div class="row">
        <div class="col-xl-12">
          {{# modal-header }}
            {{> felles-komponenter-modal-header }}
          {{/ modal-header }}
          {{# sub-header }}
            {{> organismer-subheader }}
          {{/ sub-header }}
        </div>
      </div>
      <div class="row">       
        {{^ modal-content-wide }}
          <div class="col-xl-10 offset-xl-1 a-p-static">
        {{/ modal-content-wide }}
        {{# modal-content-wide }}
          <div class="col-12 a-p-static">
        {{/ modal-content-wide }}
          {{# modal-navbar-override }}
            {{> felles-komponenter-modal-navbar }}
          {{/ modal-navbar-override }}
          {{# kontaktinformasjon-virksomhet-mangler }}
            <div class="a-modal-navbar">
            </div>
          {{/ kontaktinformasjon-virksomhet-mangler }}
          <div class="a-modal-content-target">
            <div class="a-page a-current-page">
              <div class="modalPage">
                <div class="modal-content">
                  {{^ hide-modal-body-header }}
                    {{> molekyler-modal-body-header }}
                  {{/ hide-modal-body-header }}

                  {{# show-client-delegation-content }}
                    {{> organismer-modal-klientdelegering }}
                  {{/ show-client-delegation-content }}
                  {{# show-others-with-rights-content }}
                    {{# others-with-rights }}
                      {{> organismer-modal-andre-med-rettigheter }}
                    {{/ others-with-rights }}
                  {{/ show-others-with-rights-content }}
                  {{# show-receipt-content }}
                    {{> organismer-modal-kvittering }}
                  {{/ show-receipt-content }}
                  {{# show-consent-details}} 
                    {{> organismer-se-og-fjern-samtykke }}
                  {{/ show-consent-details}}
                  {{# show-be-om-tilgang}} 
                    {{> organismer-be-om-tilgang }}
                  {{/ show-be-om-tilgang}}
                  {{# show-forward-email }}
                    {{> organismer-videresend-pa-epost }}
                  {{/ show-forward-email }}
                  {{# show-forward-email-too-large }}
                    {{> organismer-videresend-pa-epost-for-stor }}
                  {{/ show-forward-email-too-large }}
                  {{# show-actor-list-with-search }}
                    {{> organismer-aktorliste-med-sokefelt }}
                  {{/ show-actor-list-with-search }}
                  {{# show-forward-message }}
                    {{> organismer-videresend-i-altinn-melding }}
                  {{/ show-forward-message }}
                  {{# show-activitylog }}
                    {{> organismer-delegering-aktivitetslogg }}
                  {{/ show-activitylog }}
                  {{# show-error-pages }}
                    {{> organismer-modal-feilsider }}
                  {{/ show-error-pages }}
                  {{# forward-in-altinn }}
                      {{> organismer-videresend-i-altinn-tildel-enkeltrettigheter }}
                  {{/ forward-in-altinn }}
                  {{# kontaktinformasjon-virksomhet-mangler }}
                      {{> organismer-kontaktinformasjon-virksomhet-mangler}}
                  {{/ kontaktinformasjon-virksomhet-mangler }}
                  {{# show-felles-varslingsadresse }}
                    {{> organismer-felles-varslingsadresse}}
                  {{/ show-felles-varslingsadresse }}
                  {{# show-videresend-til-dpi-info }}
                      {{> organismer-videresend-til-dpi-info }}
                  {{/ show-videresend-til-dpi-info }}
                  {{# show-oppdater-lagrede-sok }}
                      {{> organismer-oppdater-lagrede-sok }}
                  {{/ show-oppdater-lagrede-sok }}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

{{^ hide-backdrop }}
<!-- DO NOT INCLUDE IN IMPLEMENTATION, ADDED BY BOOTSTRAP WHEN MODAL IS TRIGGERED -->
<div class="modal-backdrop fade show"></div>
<!-- END DO NOT INCLUDE -->
{{/ hide-backdrop }}

