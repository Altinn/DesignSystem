<div class="a-page">
	<span class="a-js-bodyClassPersist" data-body="{{ bodyClass }}"></span>
	{{> organismer-header }}

		<div class="container" id="content" tabindex="-1">

			<div class="row mt-3 mb-3">
				<div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
					{{# article-search }}
						{{> felles-komponenter-tekst-input }}
					{{/ article-search }}
				</div>
			</div>

			{{> molekyler-toppoppgaver }}

			<div class="row">
				<div class="col-sm-12 col-lg-12 mt-4  a-p-x9-xl">
						{{> felles-komponenter-sidetittel:a-h2|a-fontReg }}
				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-12 mb-4 ">

					{{# messages }}
						{{> organismer-ekspanderbart-panel-trekkspill:a-inbox-messageList }}
					{{/ messages }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			{{# inbox-starteenk-onboarding }}
				{{> organismer-onboarding-start }}
			{{/ inbox-starteenk-onboarding }}


		{{> organismer-footer }}

	</div> <!-- END .container -->

</div> <!-- END .a-page -->
