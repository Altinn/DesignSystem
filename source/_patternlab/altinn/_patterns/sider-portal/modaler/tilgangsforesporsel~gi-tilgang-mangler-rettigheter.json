{
  "show-be-om-tilgang": false,
  "show-receipt-content": true,
  "bodyClass": "a-modal-background-error",
  "icontext": "Rettighetsforespørsel",
  "hasIcon": {
    "icon-prefix": "reg",
    "iconClass": "reg-forward",
    "iconExtraClass": "a-icon"
  },
  "modal-kvittering-content-header": "Du har ikke nødvendige rettigheter til å utføre denne operasjonen",
  "modal-kvittering-content": "Siden du ikke har rettighet til <b>B01 B-melding</b> kan du ikke gi rettighet videre.",
  "modal-kvittering-second-content": false,
  "modal-kvittering-rights-list": false,

  "alert-bottom-button-row": {
    "buttons":[    
      {
        "aria-label":  false,
        "button-text": "Ok",
        "buttonClass": "a-btn a-btn-success mr-1",
        "onclick": "location.href='link.sider-portal-profil-roller-rettigheter'",
        "icon-prefix": false,
        "iconClass": false,
        "display-as-link": true,
        "enable-popover": false
      },
      {
        "aria-label":  false,
        "display-as-link": true,
        "button-text": "Avslå forespørsel",
        "buttonClass": "a-btn-link a-linkDanger a-js-popover-forceFocus",
        "icon-prefix": false,
        "iconClass": false,
        "enable-popover": true,
        "popover-placement": "bottom",
        "popover-content-id": "sampleIdPopoverButtoinLink",
        "popover-class": "popover-warning",
        "popover-text": "Rettighetsforespørselen blir slettet",
        "popover-buttons": [
          {
            "button-text": "Ok",
            "buttonClass": "a-btn a-btn-danger",
            "onclick": "location.href='link.sider-portal-profil-roller-rettigheter'"
          },
          {
            "button-text": "Avbryt",
            "buttonClass": "a-btn-link a-linkDanger",
            "display-as-link": true,
            "onclick": "$('button[aria-describedby=' + $(this).parent().parent().attr('id') + ']').click();"
          }
        ]
      }
    ]
  },
  "links": false
}
