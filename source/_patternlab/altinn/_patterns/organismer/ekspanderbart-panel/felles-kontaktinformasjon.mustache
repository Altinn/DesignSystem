<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{isExpanded}}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{ collapseID }}">
      {{# collapseHeader }}
        {{> felles-komponenter-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  <div id="{{ collapseID }}" class="a-collapsePanel-body collapse {{{ isVisibleFromStart }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }} role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
    <div class="a-collapsePanel-body-content">
      <div class="row">
        <div class="col-12">
            {{ shared-contactinfo-text }}
        </div>
        <div class="col-12">
          {{ #shared-contactinfo-link }}
            {{> felles-komponenter-link-ekstern}}<span>{{ helplinktext }}</span>
          {{ /shared-contactinfo-link }}
        </div>
        <div class="col-12">
          {{ contactinfo-warning-message }}
        </div>
      </div>
      <div class="row">
        <div class="col-12 mt-2">
          <h3 class="a-fontBold a-fontSizeM">
            {{ other-shared-contactinfo-header }}
          </h3>
        </div>
      </div>
      {{# add-contactinfo }}
       <div class="row">
          <div class="col-12 col-md-4">
            {{# add-shared-contactinfo-first-input }}
              {{> felles-komponenter-tekst-input }}
            {{/ add-shared-contactinfo-first-input }}
          </div>
          <div class="col-12 col-md-7 pr-md-0">
            {{# add-shared-contactinfo-second-input }}
              {{> felles-komponenter-tekst-input }}
            {{/ add-shared-contactinfo-second-input }}
          </div>
          <div class="col-1 d-none d-md-block pl-md-0 {{ delete-buttonClass }}">
            {{# delete-shared-contactinfo-icon }}
              {{> felles-komponenter-knapp-ikon-tekst }}
            {{/ delete-shared-contactinfo-icon }}
          </div>
          <div class="col-12 d-md-none mb-3">
            {{# delete-shared-contactinfo-link }}
              {{> felles-komponenter-knapp-link-fare }}
            {{/ delete-shared-contactinfo-link }}
          </div>
        </div>
      {{/ add-contactinfo }}
        <div class="row">
          <div class="col-12">
            {{# add-contactinfo-row-button }}
              {{> felles-komponenter-handlingsknapp }}
            {{/ add-contactinfo-row-button }}
          </div>
        </div>
    {{# shared-contactinfo}}
      <div class="row">
        <div class="col-12 mt-2">
          <h3 class="a-fontBold a-fontSizeM">
            {{ shared-contactinfo-header }}
          </h3>
        </div>
        <div class="col-12 col-md-4">
          {{# shared-contactinfo-first-input }}
            {{> felles-komponenter-tekst-input }}
          {{/ shared-contactinfo-first-input }}
        </div>
        <div class="col-12 col-md-7 pr-md-0">
          {{# shared-contactinfo-second-input }}
            {{> felles-komponenter-tekst-input }}
          {{/ shared-contactinfo-second-input }}
        </div>
        <div class="col-12">
            {{# shared-contact-checkbox }}
              {{> felles-komponenter-avkrysningsboks }}
            {{/ shared-contact-checkbox }}
        </div>
      </div>
    {{/ shared-contactinfo}}
      <div class="row">
        <div class="col-12 mt-2">
          {{# add-contactinfo-done-button }}
            {{> felles-komponenter-knapp-suksess }}
          {{/ add-contactinfo-done-button }}
        </div>
      </div>
    </div>
  </div>
</div>
