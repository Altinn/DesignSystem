<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
  <div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{ isExpanded }}" id="{{ collapse-panel-header-id }}" role="tab">
    <a href="#" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{{ collapseID }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
      {{# collapseHeader }}
        {{> felles-komponenter-ikon-tekst }}
      {{/ collapseHeader }}
    </a>
  </div>
  <div id="{{ collapseID }}" class="a-collapsePanel-body no-background collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
    {{# activity-log }}
      <div class="pr-4 pl-2 py-1 text-sm-right a-collapsePanel-body-secondary">
        {{> felles-komponenter-link }}
      </div>
    {{/ activity-log }}

    {{# box-list-id }}
      {{> molekyler-grupperte-fremhevede-knapper }}
    {{/ box-list-id }}

    <!-- case when there are no cards with rights -->
    {{# empty-rights }}
      <div class="d-none">
        <div class="clearfix">
          {{> felles-komponenter-varsel }}
        </div>

        <div class="mt-1 mx-1">
          {{> felles-komponenter-handlingsknapp-full-bredde }}
        </div>
      </div>
    {{/ empty-rights }}
  </div>
</div>
