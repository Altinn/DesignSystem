<div class="modal-body a-modal-body">
  {{# message-info }}
    <div class="row">
      <div class="col-12 mb-3">
        <p class="a-fontSizeS d-inline">
          {{ info-text-1 }}
        </p>
        <span class="a-fontSizeS a-fontBold">
          {{ name }}
        </span>
        <p class="a-fontSizeS d-inline">
          {{ info-text-2 }}
        </p>
      </div>
    </div>
  {{/ message-info }}
  <div class="row">
    <div class="col-12 col-md-11 mb-3">
      {{# textField }}
        {{> felles-komponenter-tekstfelt }}
      {{/ textField }}
    </div>
    {{# submitBtn }}
      <div class="col-12">
        {{> felles-komponenter-knapp }}
      </div>
    {{/ submitBtn }}
  </div>
</div>