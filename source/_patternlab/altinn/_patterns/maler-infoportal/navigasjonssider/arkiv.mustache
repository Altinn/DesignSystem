<div class="a-page">
	{{> organismer-header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> felles-komponenter-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<section id="content" tabindex="-1">

		<div class="container">
			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">

					{{# article-breadcrumb }}
						{{> felles-komponenter-smulesti }}
					{{/ article-breadcrumb }}

					{{> felles-komponenter-sidetittel }}

				</div>
			</div>

			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3 {{articlesContainerClass}}">

					{{# list-article }}
							{{> felles-komponenter-lenketittel-med-forklaring }}
					{{/ list-article }}

					{{# list-article-loadmore }}
				    {{> felles-komponenter-handlingsknapp }}
				  {{/ list-article-loadmore }}

				</div>
			</div>

			<div class="row">
				<div class="col-sm-12 col-md-10 col-lg-8 offset-md-1 offset-lg-2 pb-3">
					{{> felles-komponenter-bildeblokk:mt-4 }}
				</div>
			</div>

		</div> <!-- END Container -->

	</section>

	{{> organismer-footer }}
</div>
