<div class="a-page">
	{{> organismer-header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> felles-komponenter-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<section id="content" tabindex="-1">

		<div class="container a-searchResults a-offset-alignContent">
				<div class="row">
					<div class="col-12 col-lg-10 offset-lg-1">
						{{# searchWhere}}
							{{> molekyler-faner-og-knapp }}
						{{/ searchWhere}}
						<h1 class="a-h3 a-fontReg mt-2"><b>90 treff</b> i starte og drive bedrift på <b>"MVA"</b></h1>
					</div>
				</div>

				<div class="row">
					<div class="col-12 px-0 px-sm-1">
						<form class="a-searchResults-filter">
							<fieldset>
								<legend class="a-legend">Begrens treffet:
								</legend>
								<div class="a-searchResults-filter-options">
									{{# filter-search }}
										{{> felles-komponenter-valgknapp-ett-alternativ:a-switch--small }}
									{{/ filter-search }}
								</div>
							</fieldset>
						</form>
					</div>
				</div>

				{{# hit-article-1 }}
				<div class="row mx-0 pb-0 a-borderBottomDark {{pt}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> felles-komponenter-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-1 }}

				{{# infoAboutMail }}
				<div class="row py-3">
					<div class="col-sm-12 col-lg-6">
							<h2>33 treff på <b>"MVA"</b> i skjema:</h2>
							{{# suggestionList }}
								{{> felles-komponenter-liste-unummerert:pr-md-2|pb-2 }}
							{{/ suggestionList }}
							{{# suggestionList-link }}
                {{> felles-komponenter-link:a-link-large }}
              {{/ suggestionList-link }}
					</div>
					<div class="col-sm-12 col-lg-6 pt-3 pt-lg-0">
							<h2>Søk etter <b>"MVA"</b> i innboksen:</h2>
							{{> felles-komponenter-bildeblokk:a-cardImage-border }}
					</div>
				</div>
				{{/ infoAboutMail }}

				{{# hit-article-3 }}
				<div class="row mx-0 pt-2 pb-0 a-borderBottomDark {{a-searchResults-lastOfType}}">
					<div class="col-sm-12 col-lg-10 offset-lg-1 px-0">
							{{> felles-komponenter-lenketittel-med-forklaring }}
					</div>
				</div>
				{{/ hit-article-3 }}

				{{# loadmoreSearch }}
				<div class="row">
					<div class="col-sm-12">
							{{> felles-komponenter-handlingsknapp }}
					</div>
				</div>
				{{/ loadmoreSearch }}


			</div> <!-- END Container -->

	</section>

	{{> organismer-footer }}
</div>
