<div class="modal-content">
    <div class="modal-header a-modal-header">
        <div class="a-iconText a-iconText-background a-iconText-large">
            <div class="a-iconText-icon">
                <i class="ai ai-send
                 a-icon" aria-hidden="true"></i>
            </div>
            <h1 class="a-iconText-text mb-0">
                 {{# small-modal-title }}<span class="a-iconText-text-small">{{ small-modal-title }}</span>{{/ small-modal-title }}
                <span class="a-iconText-text-large">Rediger tjenesteutgave</span>
            </h1>
        </div>
    </div>
    <div class="modal-body a-modal-body">

        <form data-toggle="validator" id="contactForm">
          <div class="row">
            <div class="col-12">

              <h2 class="a-h3">Hva vil du gj√∏re med denne utgaven?</h2>
              <ul class="nav nav-tabs custom-radio a-radioButtons a-navTabsRadio" role="tablist">
                {{# radiotab-items }}
                <li class="nav-item">
                  <a class="nav-link" data-toggle="tab" href="#{{radiotab-id}}" role="tab">
                    <div class="custom-control custom-radio a-custom-radio">
                      <span class="custom-control-label">{{ radiotab-title }} </span>
                    </div>
                  </a>
                </li>
                {{/ radiotab-items }}
              </ul>

              <div class="tab-content">
                {{# radiotab-panes }}
                <div class="tab-pane" id="{{radiotab-id}}" role="tabpanel">
                  {{# nextButton }}
                    {{> felles-komponenter-knapp:mt-1 }}
                  {{/ nextButton }}
                </div>
                {{/ radiotab-panes }}
              </div>
        
                  
            </div>
          </div>
        </form>
</div>
