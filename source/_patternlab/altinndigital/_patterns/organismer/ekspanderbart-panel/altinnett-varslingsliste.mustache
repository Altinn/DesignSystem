<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
	<div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{ isExpanded }}" id="{{ collapse-panel-header-id }}" role="tab">
		<a href="#" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="{{{ collapseID }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
			{{# collapseHeader }}
				{{> felles-komponenter-ikon-tekst }}
			{{/ collapseHeader }}
		</a>
	</div>
	<div id="{{ collapseID }}" class="a-collapsePanel-body a-collapsePanel-body--border no-background collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-header-id }}">
		<div class="panel-collapse-content-section a-bgWhite">
			{{# addAlertLink }}
				<div class="mt-2 px-2">
					{{> felles-komponenter-handlingsknapp }}
				</div>
			{{/ addAlertLink }}

			{{# scheduledAlerts }}
				{{> molekyler-altinnett-liste-varslinger }}
			{{/ scheduledAlerts }}

			{{# previousAlerts }}
				{{> molekyler-altinnett-liste-varslinger }}
			{{/ previousAlerts }}
		</div>
	</div>
</div>

{{# addAlertModal }}
	{{> felles-komponenter-modal }}
{{/ addAlertModal }}

{{# editAlertModal }}
	{{> felles-komponenter-modal }}
{{/ editAlertModal }}
