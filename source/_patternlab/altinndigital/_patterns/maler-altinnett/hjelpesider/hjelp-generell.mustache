<div class="a-page">
    {{# header }}
        {{> organismer-header-altinnett }}
    {{/ header }}

    <div class="container mb-8 pb-3">
        <div class="row">
            <div class="col-sm-10 offset-sm-1">
                <div class="mb-6">
                    {{# pageTitle }}
                        {{> felles-komponenter-sidetittel }}
                    {{/ pageTitle }}
                    {{> felles-komponenter-avsnitt }}
                    {{# linkText }}
                        {{> felles-komponenter-link }}
                    {{/ linkText }}
                </div>

                <div class="mb-1 mb-md-3">
                    {{> felles-komponenter-seksjonstittel }}
                </div>
            </div>
        </div>

        {{# trekkspill }}
            {{> felles-komponenter-trekkspill-overskrifter }}
        {{/ trekkspill }}
    </div>

    {{# footer }} 
        {{> organismer-footer-altinnett }}
    {{/ footer }}
</div>