<div class="a-page">
	{{# header }}
		{{> organismer-header-altinnett }}
	{{/ header }}

	<div class="container">
		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
				{{# article-search }}
						{{> felles-komponenter-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
	</div>

	<main id="content" tabindex="-1">

		<div class="container">

			<div class="row">
				<div class="col-lg-11 offset-lg-1">
					{{> felles-komponenter-sidetittel:pb-1 }}
				</div>
			</div>

		</div>

		<div class="container-fluid">
			{{# starteogdrive-jumbo }}
					{{> felles-komponenter-jumbotron }}
			{{/ starteogdrive-jumbo }}
		</div>

		<div class="container">
			<div class="row">
				<div class="col-sm-12">
					{{> felles-komponenter-trekkspill-overskrifter }}
				</div>
			</div>

			<div class="row">
				<div class="col-sm-12">
					{{# first-card-icon }}
							{{> felles-komponenter-ikon-link:mt-4 }}
					{{/ first-card-icon }}
				</div>
			</div>

			<div class="row py-4">
        <div class="col-lg-8 offset-lg-2 ">
  				{{# newslist }}
            {{> felles-komponenter-artikkelliste }}
          {{/ newslist }}
        </div>
      </div>

			<div class="row">
				<div class="col-md-8 offset-md-2 col-xl-6 offset-xl-3">
					{{> felles-komponenter-bildeblokk:mt-4 }}
				</div>
			</div>

		</div> <!-- END Container -->

	</main>

	{{> organismer-footer-altinnett }}
</div>
