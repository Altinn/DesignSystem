{{#hidewhenempty-start}}
<div class="hidewhenempty">
{{/hidewhenempty-start}}

	{{#hasFieldset}}
	<fieldset>
	  <legend class="sr-only">{{ switch-legend }}</legend>
	{{/hasFieldset}}

		{{# radiobuttons }}

			<div class="a-switch {{styleModifier}} {{isWide}}">

				<input {{# radiobuttons-id }}id="{{{ radiobuttons-id }}}" {{/ radiobuttons-id }}
          class="sr-only"
          type="radio"
          name="{{{ radiobuttons-name }}}"
          value="radiobutton-read"
          {{# isChecked }} checked {{/ isChecked }}
          {{# onclick }} onclick="{{{ onclick }}}" {{/ onclick }}
          {{# data-toggle }} data-toggle="{{ data-toggle }}" {{/ data-toggle }}
          {{# data-target }} data-target="{{ data-target }}" {{/ data-target }}/>

				<label for="{{{ radiobuttons-id }}}" class="a-switch-label {{{ radiobuttons-class }}}">
					{{# hasIcon }}
		        {{> felles-komponenter-ikon }}
		      {{/ hasIcon }}
	        {{{radiobuttons-title}}}
	      </label>

			</div>

		{{/ radiobuttons }}

	{{#hasFieldset}}
	</fieldset>
	{{/hasFieldset}}

{{#hidewhenempty-end}}
</div>
{{/hidewhenempty-end}}
