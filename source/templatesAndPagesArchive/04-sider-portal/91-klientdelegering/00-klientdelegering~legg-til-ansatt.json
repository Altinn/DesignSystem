        "isRequired": true,
        "error-message-popover": true,
        "margin-override": "mb-0",
        "extra-help": false,
        "isHidden": false,
        "isLarge": false,
        "form-label-hasIcon": false,
        "form-input-hasButton": false
    },
    "legg-til-knapp": {
        "button-text": "Neste",
