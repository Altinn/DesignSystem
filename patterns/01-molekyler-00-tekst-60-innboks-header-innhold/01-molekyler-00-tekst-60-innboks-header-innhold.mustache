<!-- START: 01-molekyler-00-tekst-60-innboks-header-innhold JS doc: setupTruncateLines.js, togglePanel.js -->
<div class="row a-inboxHeadingContent{{# extra-class-headingContent }} {{ extra-class-headingContent }}{{/ extra-class-headingContent }}">
  {{# heading-icon }}
  <div class="col-auto a-msgIconWrapper">
    {{> atomer-ikon }} {{# heading-secondary-icon }} {{> atomer-ikon }} {{/ heading-secondary-icon }}
  </div>
  {{/ heading-icon}} {{# heading-tekst }}
  <div class="col a-msgHeadingTextContainer">
    <div class="d-inline-block">
      {{# due-date-content }}
      <div class="col-auto hidden-md-up a-msgDuedate">
        <span class="a-fontBold">{{ due-date-label }}</span> {{ due-date }}
      </div>
      {{/ due-date-content }} {{# state-content }}
      <div class="col-auto hidden-md-up a-msgState">
        <span{{# state-class }} class="{{ state-class }}" {{/ state-class }}>{{ state-text }}</span>
      </div>
      {{/ state-content }} {{^ is-h1 }}
      <h3 class="mb-0 a-js-truncate-2-sm-down">
        {{ textDescription }}
        <span class="a-msgTitle">{{ textMain }}</span>
      </h3>
      {{/ is-h1 }} {{# is-h1 }}
      <h1 class="mb-0 a-js-truncate-2-sm-down">
        {{ textDescription }}
        <span class="a-msgTitle">{{ textMain }}</span>
      </h1>
      {{/ is-h1 }}

      <div class="a-fontSizeXXS">{{ textSmall }}</div>
    </div>
  </div>
  {{/ heading-tekst }} {{# due-date-content }} {{# past-due-date }}
  <div class="col-auto hidden-sm-down a-msgDuedateIcon">
    {{> atomer-ikon }}
  </div>
  {{/ past-due-date }}
  <div class="col-auto hidden-sm-down a-msgDuedate">
    <span class="a-fontBold">{{ due-date-label }}</span> {{ due-date }}
  </div>
  {{/ due-date-content }} {{# state-content }}
  <div class="col-auto hidden-sm-down a-msgState">
    <span{{# state-class }} class="{{ state-class }}" {{/ state-class }}>{{ state-text }}</span>
  </div>
  {{/ state-content }} {{# shortcuts }}
  <div class="col-auto hidden-sm-down a-msgShortcutsContainer">
    {{# shortcut-icons }}
    <div{{# button-class }} class="{{ button-class }}" {{/ button-class }}{{^ disabled }} role="button" tabindex="0" {{/ disabled }}{{# aria-label }} aria-label="{{ aria-label }}" {{/ aria-label }}{{# aria-hidden }} aria-hidden="true" {{/ aria-hidden }}>
      {{# button-icon }} {{> atomer-ikon }} {{/ button-icon }}
  </div>
  {{/ shortcut-icons }}
</div>
{{/ shortcuts }}
</div>
<!-- END: 01-molekyler-00-tekst-60-innboks-header-innhold -->