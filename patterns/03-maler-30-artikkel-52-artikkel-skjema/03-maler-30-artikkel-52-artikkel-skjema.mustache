<div class="a-page" id="smoothState">

	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-2">
					{{> molekyler-smulesti }}
				</div>
			</div>

			{{> molekyler-kort-hoved }}
			<div class="row a-text">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-2">
					<h2>{{{ header-2 }}}</h2>
					{{> molekyler-trekkspill }}
				</div>
			</div>
			<div class="row">
				<div class="col-sm-6 col-lg-5 offset-lg-1 py-2">
					<h2>{{{ header-3 }}}</h2>
					{{# help }}
						{{{ help-prefix }}}
						{{> atomer-link:a-fontBold }}
						{{{ help-end }}}
					{{/ help }}
				</div>
			</div>

		</div> <!-- END Container -->

		{{> organismer-footer }}

</div>

