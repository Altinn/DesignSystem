<div class="row">
  {{# login-info-header }}
  <div class="col-12">
    <h3 class="a-fontBold a-fontSizeM">
      {{ login-info-header }}
    </h3>
  </div>
  {{/ login-info-header }}
  <div class="col-12 col-md-5">
    {{# login-info-username-input }} {{> atomer-input }} {{/ login-info-username-input }}
  </div>
  <div class="col-12 col-md-5 pt-md-3 pt-sm-0 mb-1 mb-sm-0 a-js-password-link">
    {{# login-info-password-link }} {{> atomer-knapp }} {{/ login-info-password-link }}
  </div>
  <div class="col-12 col-md-5" style="display: none">
    {{# login-info-password-input }} {{> atomer-input }} {{/ login-info-password-input }}
  </div>
  <div class="col-12 col-md-5">
    {{# login-info-email-input }} {{> atomer-input }} {{/ login-info-email-input }}
  </div>
</div>
{{# login-info-checkbox }}
<div class="row">
  <div class="col">
    {{> atomer-avkrysningsboks }}
  </div>
</div>
{{/ login-info-checkbox }}
<div class="row">
  <div class="col mt-1">
    {{# login-info-save-btn }} {{> atomer-knapp }} {{/ login-info-save-btn }}
  </div>
</div>
<!-- END: 02-organismer-49-ekspanderbart-panel-innhold-10-innloggingsinformasjon-innhold -->