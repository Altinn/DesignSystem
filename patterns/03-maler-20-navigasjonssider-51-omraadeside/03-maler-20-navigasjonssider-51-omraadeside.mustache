<div class="a-page" id="smoothState">

	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 py-3">
				{{# article-search }}
					{{> atomer-tekst-input }}
				{{/ article-search }}
			</div>
		</div>
		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1 pb-2">
				{{> molekyler-smulesti }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">
				{{> atomer-sidetittel }}
				{{> atomer-byline }}
				{{> atomer-avsnitt }}
			</div>
		</div>
		<div class="row">
			<div class="col-sm-12 col-lg-12 offset-lg-0 pb-3">
				{{> organismer-grupperte-kort }}
			</div>
		</div>
		<div class="row">
			<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">
				<h2>{{{ header-2 }}}</h2>
				{{> molekyler-trekkspill }}
			</div>
		</div>
		<div class="row">
			<div class="col-sm-6 col-lg-5 offset-lg-1 py-3">
				<h2>{{{ header-3 }}}</h2>
				{{# help }}
					{{{ help-prefix }}}
					{{> atomer-link }}
				{{/ help }}
			</div>
			<div class="col-sm-6 col-lg-5 offset-lg-0 py-3">
				<h2>{{{ header-4 }}}</h2>
				{{# help-links }}
				  {{> atomer-knapp-ikon-tekst }}
			  {{/ help-links }}
			</div>
		</div>


		{{> organismer-footer }}

	</div> <!-- END Container -->

</div>
