          <div class="col">
            {{# personswitch-current }} {{> molekyler-person-privat }} {{/ personswitch-current }}
          </div>
          <div class="col-auto">
            {{> atomer-link(link-class: "a-link-logout", link-text: "Logg ut", link-url: "/patterns/04-sider-01-forside-00-forside/04-sider-01-forside-00-forside.html") }}
          </div>
        </div>
        {{/ is-first-time }}
        <div class="row px-lg-3 pb-2">
          <div class="col-12 col-lg-4 mb-1">
