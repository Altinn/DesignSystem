<div class="form-group input-group transparent a-displayInlineBlockXs a-form-group">
  {{# show-label }}
  <div class="a-fullWidthXs mb-1 mb-sm-0">
    <span class="a-form-label-inline a-fontSizeL mr-2 a-height-3x">{{ label-text }}</span>
  </div>
  {{/ show-label }}
  <div class="dropdown{{# isTransparent }} a-transparent {{/ isTransparent }}a-fullWidthXs">
    <button class="btn dropdown-toggle a-dropdown-toggle" type="button" id="dropdownMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        {{> atomer-ikon }}
        <span class="a-form-text">
          <b>{{ dropdown-name }}</b> {{# dropdown-number-prefix }} <span class="hidden-xs-down">{{ dropdown-number-prefix }}</span> {{/ dropdown-number-prefix }} {{dropdown-number}}
        </span>
      </button>
    <div class="dropdown-menu a-dropdown-menu" aria-labelledby="dropdownMenu1">
      <a class="dropdown-item a-dropdown-item a-dropdown-item-active" href="#">
          {{> atomer-ikon }}
          <span class="a-form-text ">
          <b>{{ dropdown-name }}</b> {{# dropdown-number-prefix }} <span class="hidden-xs-down">{{ dropdown-number-prefix }}</span> {{/ dropdown-number-prefix }} {{dropdown-number}}
          </span>
        </a> {{# dropdown-items }}
      <a class="dropdown-item a-dropdown-item" href="#">
            {{> atomer-ikon }}
            <span class="a-form-text">
              <b>{{ dropdown-item-name }}</b> {{# dropdown-item-number-prefix }} <span class="hidden-xs-down">{{ dropdown-item-number-prefix }}</span> {{/ dropdown-item-number-prefix }} {{ dropdown-item-number }}
            </span>
          </a> {{/ dropdown-items }}
    </div>
  </div>
</div>

<!-- END: 00-atomer-01-forms-50-flervalg-stilisert -->