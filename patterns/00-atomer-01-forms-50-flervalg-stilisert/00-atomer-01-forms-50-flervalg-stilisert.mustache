<div class="form-group input-group transparent a-form-group">
  <div class="dropdown{{# isTransparent }} a-transparent {{/ isTransparent }}a-fullWidthXs">
    <button class="btn dropdown-toggle a-dropdown-toggle" type="button" id="dropdownMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
      {{> atomer-ikon }}
      <span class="a-form-text">
        {{ dropdown-name }} 
      </span>
    </button>
    <div class="dropdown-menu a-dropdown-menu" aria-labelledby="dropdownMenu">
      {{# dropdown-items }}
      <a class="dropdown-item a-dropdown-item" href="#">
          {{> atomer-ikon }}
          <span class="a-form-text">
            {{ dropdown-item-name }} 
          </span>
        </a> {{/ dropdown-items }}
    </div>
  </div>
</div>

