<div class="a-collapse">
  <div class="{{# collapse-header-class }}{{ collapse-header-class }}{{/ collapse-header-class }}">
    <h2 class="a-collapseHeader a-h3{{# heading-action }} a-collapseHeader-with-actions{{/ heading-action }}">
      <a data-toggle="collapse" href="#" data-target="#{{{ collapse-header-contentID }}}" {{# data-parent }} data-parent="#{{ data-parent }}" {{/ data-parent }} aria-expanded="{{ aria-expanded-header }}" aria-controls="{{{ collapse-header-contentID }}}" class="a-collapse-title a-collapse-title--absolute no-decoration {{# collapsed }} collapsed {{/ collapsed }}">
        <span class="a-dropdownArrow"></span> <span>{{{ collapse-header-title }}}</span> <span class="a-js-none">{{{ collapse-header-title-none }}}</span>
      </a>
    </h2>
    {{# heading-action }}
    <div class="a-list-heading-action" tabindex="0">
      {{# heading-text }}
      <span class="{{^ heading-text-class }}hidden-sm-down{{/ heading-text-class }}{{# heading-text-class }} {{ heading-text-class}} {{/ heading-text-class }}">
          {{ heading-text }}
        </span> {{/ heading-text }} {{> atomer-ikon }}
    </div>
    {{/ heading-action }}
  </div>
</div>

