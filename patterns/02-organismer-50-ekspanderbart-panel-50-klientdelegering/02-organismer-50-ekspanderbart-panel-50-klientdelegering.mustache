<!-- START: 02-organismer-50-ekspanderbart-panel-50-klientdelegering -->
<div class="card a-accordion-card a-collapsePanel {{{ styleModifier }}}">
	<div class="card-header a-js-index-heading a-collapsePanel-heading collapsed {{ isExpanded }}" role="tab">
		<a href="#{{ collapseID }}" tabindex="0" data-toggle="collapse" data-target="#{{{ collapseID }}}" aria-expanded="false" aria-controls="#{{{ collapseID }}}" {{# dataParent }}data-parent="{{{dataParent}}}"{{/ dataParent }}>
			{{# collapseHeader }}
				{{> molekyler-ikon-tekst }}
			{{/ collapseHeader }}
		</a>
	</div>
	<div id="{{ collapseID }}" class="a-collapsePanel-body a-collapsePanel-body--border no-background collapse {{{ isVisibleFromStart }}}" role="tabpanel" aria-labelledby="{{ collapse-panel-id }}">
		<div class="panel-collapse-content-section px-2 pb-2 a-px-1-xs-down a-pb-1-xs-down">
			<div class="a-first-action pt-3 a-pt-2-xs-down">
				{{> atomer-knapp-veksle }}
			</div>
			{{# klientdelegering-file-desktop }}
      <div class="a-third-action hidden-md-down pt-3">
        <div class="third-action-spacer">
          {{> atomer-knapp }}
        </div>
      </div>
			{{/ klientdelegering-file-desktop }}
			<div class="a-second-action pt-3 a-pt-2-xs-down" id="searchForClient" style="display: none;">
				{{# sok-etter-klient }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-klient }}
			</div>
			<div class="a-second-action pt-3 a-pt-2-xs-down" id="searchForPerson">
				{{# sok-etter-person }}
					{{# display }}
						{{> atomer-tekst-input }}
					{{/ display }}
				{{/ sok-etter-person }}
			</div>
			<div class="a-fourth-action mt-3 a-mt-2-xs-down" id="searchHiddenForClient">
        {{# klientdelegering-add }}
          {{> atomer-handlingsknapp-full-bredde }}
        {{/ klientdelegering-add}}
			</div>
		</div>
		<div>
		<div id="firstTable">
			{{# employee-table }}
        {{> molekyler-liste-unummerert-ny }}
			{{/ employee-table }}
		</div>
		<div id="secondTable" style="display: none;">
			{{# clients-table }}
        {{> molekyler-liste-unummerert-ny }}
			{{/ clients-table }}
		</div>
	</div>
  {{# klientdelegering-file-mobile }}
  <div class="a-third-action-mobile hidden-lg-up">
    <div class="a-third-action-spacer">
      {{> atomer-knapp }}
    </div>
  </div>
  {{/ klientdelegering-file-mobile }}
</div>

<!-- END: 02-organismer-50-ekspanderbart-panel-50-klientdelegering -->
