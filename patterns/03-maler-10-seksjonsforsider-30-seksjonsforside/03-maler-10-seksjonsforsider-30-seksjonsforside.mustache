<div class="a-page" id="smoothState">
	{{> organismer-header }}

	<div class="container" id="content" tabindex="-1">

		<div class="row">
			<div class="col-sm-12 col-lg-6 offset-lg-3 pt-3 mb-5">
				{{# article-search }}
						{{> atomer-tekst-input }}
				{{/ article-search }}

			</div>
		</div>

		<div class="row">
			<div class="col-sm-11 offset-sm-1">
				{{> atomer-sidetittel:a-blueDarkerText|pb-2 }}
			</div>
		</div>

	</div>

	<div class="container-fluid">
		{{# starteogdrive-jumbo }}
				{{> molekyler-jumbotron }}
		{{/ starteogdrive-jumbo }}

	</div>


	<div class="container">

		<div class="row">
			<div class="col-sm-12">
				{{> molekyler-trekkspill-overskrifter }}
			</div>
		</div>

		<div class="row">
			<div class="col-sm-12">
				{{# first-card-icon }}
						{{> molekyler-ikon-link:mt-4 }}
				{{/ first-card-icon }}
			</div>
		</div>

		<div class="row">
			<div class="col-md-8 offset-md-2 col-xl-6 offset-xl-3">
				{{> molekyler-bilde-innhold:mt-4 }}
			</div>
		</div>

		{{> organismer-footer }}

	</div> <!-- END Container -->

</div>
