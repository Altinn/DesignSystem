<div class="a-page" id="smoothState">
	{{> organismer-header }}

	{{# jumbotron }}
		{{> molekyler-jumbotron }}
	{{/ jumbotron }}

	<div class="container" id="content" tabindex="-1">

		{{# artikkelsok }}
			<div class="row">
				<div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
					{{# article-search }}
						{{> atomer-tekst-input }}
					{{/ article-search }}
				</div>
			</div>
		{{/ artikkelsok }}

			<div class="row">
				<div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">

					{{# smulesti }}
						{{> molekyler-smulesti }}
					{{/ smulesti }}

					{{> atomer-sidetittel }}

					{{> atomer-byline }}

					<p class="a-leadText">{{{ article-intro }}}</p>

				</div>
			</div>

			<div class="row">
				<div class="col-sm-12 col-lg-12 mb-5">

					{{> molekyler-sammenligning-header }}

					{{# first-compare }}
						{{> organismer-sammenligning }}
					{{/ first-compare }}

					{{# second-compare }}
						{{> organismer-sammenligning }}
					{{/ second-compare }}

					{{# third-compare }}
						{{> organismer-sammenligning }}
					{{/ third-compare }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->

			<div class="row">
				<div class="col-sm-12 col-lg-6 mb-2">

					{{# first-card-icon }}
							{{> molekyler-ikon-link }}
					{{/ first-card-icon }}

				</div><!-- END .col-... -->

				<div class="col-sm-12 col-lg-6 mb-2">

					{{# second-card-icon }}
							{{> molekyler-ikon-link }}
					{{/ second-card-icon }}

				</div><!-- END .col-... -->
			</div><!-- END .row -->


		</div> <!-- END Container -->

		{{> organismer-footer }}

</div>
