<div class="a-page">
  {{# header }} {{> organismer-header }} {{/ header }}
  <section id="content" tabindex="-1">

    <div class="container" id="content" tabindex="-1">
      <div class="row">
        <div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
          {{# article-search }} {{> atomer-tekst-input }} {{/ article-search }}
        </div>
      </div>

      {{# hasArticleStart }}
      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1 mb-2">
          {{> molekyler-smulesti }} {{> atomer-sidetittel }} {{> atomer-byline }} {{> atomer-intro-tekst}} {{#hasArticleStartLink}}{{> atomer-link }}{{/hasArticleStartLink}}
          <hr class="mt-2 mb-0 hidden-md-down">
        </div>
      </div>
      {{/ hasArticleStart }} {{# hasMultipleCards }}
      <div class="row">
        <div class="col-sm-12 col-lg-12 offset-lg-0 pb-3">
          {{> organismer-grupperte-kort }}
        </div>
      </div>
      {{/ hasMultipleCards }} {{# hasAccordion }}
      <div class="row">
        <div class="col-sm-12 col-lg-8 offset-lg-2 mt-3">
          <h2>{{{ header-2 }}}</h2>
          {{> molekyler-trekkspill:a-accordion-largeText }}
        </div>
      </div>
      {{/ hasAccordion }} {{# hasHelp }}
      <div class="row">
        <div class="col-sm-12 col-lg-8 offset-lg-2 py-4">
          <h2>{{{ header-3 }}}</h2>
          <p class="a-paragraphLarge">
            {{{ help-prefix }}} {{> atomer-link:a-fontBold }} {{{ help-end }}}
          </p>
        </div>
      </div>
      {{/ hasHelp }}

    </div>
    <!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>

