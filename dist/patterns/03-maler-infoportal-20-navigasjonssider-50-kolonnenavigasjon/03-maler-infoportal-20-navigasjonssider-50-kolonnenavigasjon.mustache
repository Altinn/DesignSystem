<div class="a-page">
  {{> organismer-header }}

  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
        {{# article-search }} {{> atomer-tekst-input }} {{/ article-search }}
      </div>
    </div>
  </div>

  <section id="content" tabindex="-1" class="a-containerColnav">

    <div class="container">
      <div class="a-loader a-js-drilldownLoader">
        <div class="loader loader-ellipsis"></div>
      </div>
      <div class="row a-containerColnav-top">
        <div class="col-sm-12 col-md-6 col-lg-9">
          <h1>
            <span class="a-fontBold a-js-colnavTitleBold"></span>
            <span class="a-js-colnavTitleRegular">Alle skjemaer</span>
          </h1>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-3" data-switchUrl1="getcategory" data-switchUrl2="getprovider" data-switchEndpoint="http://altinn.github.io/DesignSystem/data/">
          <!-- http://altinn-dev.dev.bouvet.no/api/ -->
          {{> atomer-knapp-veksle }}
        </div>
      </div>

    </div>
    <div class="container a-container-fluid-small">

      <div class="a-contentOverview">
        <div class="row">
          {{> molekyler-meny-kolonner }}
          <div class="col-sm-12 col-lg-6 a-containerColnav-right">
            <h2 class="pt-5">Aktuelle skjema og tjenester</h2>
            {{# suggestionList }} {{> molekyler-liste-unummerert }} {{/ suggestionList }}
          </div>
        </div>
      </div>

    </div>
    <!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>

<!-- END: 03-maler-infoportal-20-navigasjonssider-50-kolonnenavigasjon -->