{{# show-mainheader }} {{> organismer-header }} {{/ show-mainheader }} {{# show-subheader }} {{> organismer-subheader }} {{/ show-subheader }}
<div class="container">
  <div class="row mt-3 mb-3">
    <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
      {{# article-search }} {{> atomer-tekst-input }} {{/ article-search }}
    </div>
  </div>

  {{# sidebar }}
  <div class="row">
    <div class="col-md-3 col-12">
      <div class="hidden-sm-down my-3">
        {{# new-form }} {{> atomer-handlingsknapp }} {{/ new-form }}
      </div>
      {{# vis-toppoppgaver }} {{> molekyler-toppoppgaver }} {{/ vis-toppoppgaver }}

      <div class="hidden-sm-down">
        <p class="a-fontSizeS mt-2 mb-1">Lagrede s√∏k</p>
        {{# vis-lagrede-sok }} {{> molekyler-toppoppgaver }} {{/ vis-lagrede-sok }}
      </div>
    </div>
    {{/ sidebar }} {{# empty-inbox }}
    <div class="row">
      <div class="col">
        <h2 class="a-fontSizeM a-fontBold mt-3 mb-0">{{ empty-inbox-heading }}</h2>
        <p class="a-fontSizeM">{{ empty-inbox-title }}</p>
        <h3 class="mb-1 mt-3 a-fontSizeXS a-fontReg">{{ empty-inbox-list-title }}</h3>
        {{> atomer-liste-nummerert }}
        <p class="mt-2">{{ empty-inbox-undertext }}{{> atomer-link }}</p>
      </div>
    </div>
    {{/ empty-inbox }} {{# search-result-menu }} {{> molekyler-faner-og-knapp }} {{/ search-result-menu }} {{# limit-hits }}
    <div class="mt-4">
      <p class="d-inline a-fontSizeM">{{ limit-hits-text }}</p>
      {{> atomer-valgknapp:a-switch--small }} {{> atomer-ikon}}
    </div>
    {{/ limit-hits }} {{# inbox-no-results }}
    <p class="a-border-bottom a-no-border-small a-fontSizeM mt-2 mb-0">{{ inbox-no-results-text }}</p>
    <div class="mt-0 mt-md-5 mx-0 mx-md-4">
      {{> organismer-innboks-ingen-treff }}
    </div>
    {{/ inbox-no-results }} {{# heading }}
    <h1 class="{{ heading-class }}">{{ heading }}</h1>
    {{/ heading }} {{# sidebar }}
    <div class="col-md-9 col-12">
      {{/ sidebar }} {{# inbox }}
      <div class="row{{# inbox-extra-class }} {{ inbox-extra-class }}{{/ inbox-extra-class }}">
        <div class="col" id="{{ accordionId }}">
          {{# elementList }}
          <div class="row">
            <div class="col-sm-12 col-lg-12{{# block-margin }} {{{ block-margin }}} {{/ block-margin }}">
              {{> atomer-seksjonstittel }}
            </div>
          </div>

          {{# messages }}
          <div class="row">
            <div class="col-sm-12 col-lg-12">
              <div {{# accordion-large-id }}id="{{ accordion-large-id }}" {{/ accordion-large-id }} class="a-panelAccordion" role="tablist">
                {{# accordion-large-items }} {{> organismer-ekspanderbart-panel }} {{/ accordion-large-items }}
              </div>
            </div>
          </div>
          {{/ messages }} {{/ elementList }}
        </div>
      </div>
      {{/ inbox }} {{# load-more-btn }}
      <div class="row">
        <div class="col">
          {{> atomer-handlingsknapp-full-bredde }}
        </div>
      </div>
      {{/ load-more-btn }} {{# sidebar }}
    </div>
  </div>
  {{/ sidebar }}
</div>
{{> organismer-footer }} {{> molekyler-hjelp-container }}
<!-- END: 03-maler-portal-50-innboks-00-innboks -->