{{# show-mainheader }} {{> organismer-header }} {{/ show-mainheader }} {{# show-subheader }} {{> organismer-subheader }} {{/ show-subheader }}
<div class="a-overlay-container d-none" tabindex="-1">
  {{> organismer-filter }}
</div>
<div class="container a-js-hideElement">
  {{# article-search }}
  <div class="row mt-3 mb-3">
    <div class="col-sm-8 offset-sm-2 col-lg-6 offset-lg-3">
      {{> atomer-tekst-input }}
    </div>
  </div>
  {{/ article-search }} {{# heading }}
  <div class="row">
    <div class="col">
      <h1 class="{{ heading-class }}">{{ heading }}</h1>
    </div>
  </div>
  {{/ heading }} {{# sidebar }}
  <div class="row {{# has-bigger-container }} bigger-container {{/ has-bigger-container}}">
    <div class="col-lg-2 col-12 mr-3 mr-lg-5 px-0">
      <div class="hidden-md-down my-3">
        {{# new-form }} {{> atomer-handlingsknapp }} {{/ new-form }}
      </div>
      {{# vis-toppoppgaver }} {{> molekyler-toppoppgaver }} {{/ vis-toppoppgaver }}

      <div class="hidden-md-down">
        <p class="a-fontSizeS mt-2 mb-1">Lagrede s√∏k</p>
        {{# vis-lagrede-sok }} {{> molekyler-toppoppgaver }} {{/ vis-lagrede-sok }}
      </div>
    </div>
    {{/ sidebar }} {{# customize-saved-searches }}
    <div class="row pt-6 pb-3">
      <div class="col">
        {{# custom-title }}
        <h1 class="a-fontSizeM">
          {{# text-items }} {{# classes }} <span class="{{ classes }}"> {{/ classes }}
                {{{ text }}}
                {{# classes }} </span> {{/ classes }} {{/ text-items }}
        </h1>
        {{/ custom-title }} {{# elements }} {{> molekyler-liste-unummerert }} {{/ elements }}
      </div>
    </div>
    {{/ customize-saved-searches }} {{# search-result-menu }} {{> molekyler-faner-og-knapp }} {{/ search-result-menu }} {{# limit-hits }}
    <div class="mt-4">
      <p class="d-inline a-fontSizeM">{{ limit-hits-text }}</p>
      {{# active-filters }} {{> atomer-merkelapp }} {{/ active-filters }} {{> atomer-knapp-ikon}}
    </div>
    {{/ limit-hits }} {{# inbox-no-results }}
    <p class="a-border-bottom a-no-border-small a-fontSizeM mt-2 mb-0">{{ inbox-no-results-text }}</p>
    {{> organismer-innboks-ingen-treff }} {{/ inbox-no-results }} {{# expanded-message-element }}
    <div class="row">
      <div class="col">
        {{> organismer-ekspanderbart-panel }}
      </div>
    </div>
    {{/ expanded-message-element }} {{# sidebar }}
    <div class="col-lg-9 col-12">
      {{/ sidebar }} {{# sticky-help }}
      <div class="row">
        <div class="col mt-3">
          {{> atomer-varsel }}
        </div>
      </div>
      {{/ sticky-help }} {{# inbox }}
      <div class="row">
        <div class="col" id="{{ accordionId }}">
          {{# elementList }}
          <div class="row  {{# element-extra-class }} {{ element-extra-class }}{{/ element-extra-class }}">
            <div class="col-sm-12 col-lg-12{{# block-margin }} {{{ block-margin }}} {{/ block-margin }}">
              {{> atomer-seksjonstittel }}
            </div>
          </div>

          {{# messages }}
          <div class="row">
            <div class="col-sm-12 col-lg-12">
              <div {{# accordion-large-id }}id="{{ accordion-large-id }}" {{/ accordion-large-id }} class="a-panelAccordion" role="tablist">
                {{# inbox-element-removed }} {{> molekyler-infotekst-med-border }} {{/ inbox-element-removed }} {{# accordion-large-items }} {{> organismer-ekspanderbart-panel }} {{/ accordion-large-items }}
              </div>
            </div>
          </div>
          {{/ messages }} {{/ elementList }}
        </div>
      </div>
      {{/ inbox }} {{# empty-inbox }}
      <h2 class="a-fontSizeM a-fontBold mt-3 mb-0">{{ empty-inbox-heading }}</h2>
      <p class="a-fontSizeM">{{ empty-inbox-title }}</p>
      <h3 class="mb-1 mt-3 a-fontSizeXS a-fontReg">{{ empty-inbox-list-title }}</h3>
      {{> atomer-liste-nummerert }}
      <p class="mt-2">{{ empty-inbox-undertext }}{{> atomer-link }}</p>
      {{/ empty-inbox }} {{# load-more-btn }}
      <div class="row">
        <div class="col">
          {{> atomer-handlingsknapp-full-bredde }}
        </div>
      </div>
      {{/ load-more-btn }} {{# sidebar }}
    </div>
  </div>
  {{/ sidebar }}
</div>
{{> organismer-footer }} {{> molekyler-hjelp-container }}
<!-- END: 03-maler-portal-50-innboks-00-innboks -->