{{# header }} {{> organismer-header }} {{/ header }}


<div class="container-fluid a-login-container a-bgWhite a-shadow" id="content" tabindex="-1">
  <div class="container a-triangleShadow">
    {{^ is-first-time }}
    <div class="row pb-2 pb-md-6 px-2 px-md-1">
      <div class="col">
        {{# personswitch-current }} {{> molekyler-person-privat }} {{/ personswitch-current }}
      </div>
    </div>
    {{/ is-first-time }}
    <div class="row px-2 px-md-1">
      <div class="col-12 col-lg-4 mb-1">
        {{# is-first-time }}
        <h1 class="a-h2 d-inline">Velg profil</h1>
        {{# headerFootnote}} {{> atomer-link-med-popover-ikon }} {{/ headerFootnote }} {{/ is-first-time }} {{^ is-first-time }}
        <h1 class="a-h2 d-inline">Alle dine profiler:</h1>
        {{/ is-first-time }}
      </div>
      <div class="col-12 col-lg-5 ">
        {{# personswitch-search }} {{> atomer-tekst-input }} {{# no-results-text }}
        <p class="a-fontBold a-fontSizeXS">
          {{ no-results-text }}
          <p>
            {{/ no-results-text }} {{/ personswitch-search }}
      </div>
      {{> atomer-link(link-class: "a-link-logout", link-text: "Logg ut", link-url: "/patterns/04-sider-infoportal-01-forside-00-forside/04-sider-infoportal-01-forside-00-forside.html") }}
    </div>

    {{# is-first-time }}
    <div class="row">
      <div class="col-sm-12{{^ is-simple }} pb-3{{/ is-simple }}">
        {{# mainPerson }} {{> molekyler-ekspanderbar-liste-status }} {{/ mainPerson }}
      </div>
    </div>
    {{/ is-first-time }}
    <div class="row">
      {{# is-first-time }} {{^ is-simple }}
      <div class="col">
        <h2 class="a-h4 px-lg-3">Alle dine profiler:</h2>
      </div>

      {{# utvidet-sok }}
      <div class="col-auto">
        {{> atomer-link-ikon }}
      </div>
      {{/ utvidet-sok }} {{/ is-simple }} {{/ is-first-time }}

      <div class="col-sm-12 pb-3">
        {{# personswitch }} {{> organismer-personbytte}} {{/ personswitch }}
      </div>
    </div>
  </div>
</div>

</section>
</div>

