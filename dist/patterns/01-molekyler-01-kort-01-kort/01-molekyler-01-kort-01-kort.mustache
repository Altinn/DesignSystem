<div class="a-card {{a-card-filter}}">
  {{# card-content }}
  <h2 class="a-h3">{{> atomer-link }}<span>{{ card-code }}</span></h2>
  <p class="a-fontBold mb-0">{{ card-title }}</p>
  <p>{{ card-description }}</p>
  {{/ card-content }} {{# card-filter }} {{> atomer-ekspanderbar-tittel }}

  <div id="{{ collapse-header-contentID }}" class="a-collapseContent collapse {{# aria-expanded }}show{{/ aria-expanded }}" aria-expanded="{{ aria-expanded }}">
    <div class="text-sm-center">
      {{# filterable-items }} {{> atomer-valgknapp }} {{/ filterable-items }}
    </div>
  </div>
  {{/ card-filter }}
</div>

<!-- END: 01-molekyler-01-kort-01-kort -->