<div class="a-page">
  {{> organismer-header }}

  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-lg-6 offset-lg-3 a-articleSearch">
        {{# article-search }} {{> atomer-tekst-input }} {{/ article-search }}
      </div>
    </div>
  </div>

  <section id="content" tabindex="-1">

    <div class="container">
      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1 pb-3">

          {{# article-breadcrumb }} {{> molekyler-smulesti }} {{/ article-breadcrumb }} {{> atomer-sidetittel }} {{# hasIntro}} {{> atomer-intro-tekst }} {{/ hasIntro}}

        </div>
      </div>

      {{# help-questions }}
      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1">
          {{> atomer-seksjonstittel }}
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12">
          {{> molekyler-trekkspill-overskrifter:a-accordion-large--offset }}
        </div>
      </div>
      {{/ help-questions }}

      <div class="row">
        <div class="col-sm-12 col-lg-10 offset-lg-1">
          {{> molekyler-bilde-innhold:mt-5 }}
        </div>
      </div>

    </div>
    <!-- END Container -->

  </section>

  {{> organismer-footer }}
</div>

<!-- END: 03-maler-infoportal-20-navigasjonssider-80-navigasjonstrekkspill -->